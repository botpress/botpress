{
  "name": "calcom",
  "version": "0.4.0",
  "title": "Cal.com",
  "description": "Schedule meetings and manage events using the Cal.com scheduling platform.",
  "user": {
    "tags": {
      "id": {
        "title": "User ID",
        "description": "The ID of the user"
      }
    }
  },
  "events": {},
  "actions": {
    "getEventTypes": {
      "title": "Get Event Types",
      "description": "Fetches all event types from Cal.com",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "username": {
              "type": "string",
              "description": "The username of the Cal.com account",
              "x-zui": {
                "title": "Username"
              }
            }
          },
          "required": ["username"],
          "additionalProperties": false,
          "x-zui": {},
          "$schema": "http://json-schema.org/draft-07/schema#"
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "eventTypes": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "number",
                    "description": "The unique identifier of the event type",
                    "x-zui": {
                      "title": "Event Type ID"
                    }
                  },
                  "lengthInMinutes": {
                    "type": "number",
                    "description": "The duration of the event type in minutes",
                    "x-zui": {
                      "title": "Duration (minutes)"
                    }
                  },
                  "title": {
                    "type": "string",
                    "description": "The title of the event type",
                    "x-zui": {
                      "title": "Title"
                    }
                  },
                  "slug": {
                    "type": "string",
                    "description": "The slug of the event type",
                    "x-zui": {
                      "title": "Slug"
                    }
                  },
                  "description": {
                    "type": "string",
                    "description": "A brief description of the event type",
                    "x-zui": {
                      "title": "Description"
                    }
                  },
                  "lengthInMinutesOptions": {
                    "type": "array",
                    "items": {
                      "type": "number",
                      "x-zui": {}
                    },
                    "description": "Available duration options for the event type",
                    "x-zui": {
                      "title": "Duration Options (minutes)"
                    }
                  }
                },
                "required": ["id", "lengthInMinutes", "title", "slug", "description", "lengthInMinutesOptions"],
                "additionalProperties": false,
                "x-zui": {}
              },
              "description": "A list of event types associated with the Cal.com account",
              "x-zui": {
                "title": "Event Types"
              }
            }
          },
          "required": ["eventTypes"],
          "additionalProperties": false,
          "x-zui": {},
          "$schema": "http://json-schema.org/draft-07/schema#"
        }
      }
    },
    "getAvailableTimeSlots": {
      "title": "Get Available Time Slots",
      "description": "Fetches available time slots for a specific event type within a date range ( default to next 7 days if not provided )",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "eventTypeId": {
              "type": "number",
              "description": "The ID of the event type",
              "x-zui": {
                "title": "Event Type ID"
              }
            },
            "startDate": {
              "type": "string",
              "description": "Start date in YYYY-MM-DD format",
              "x-zui": {
                "title": "Start Date"
              }
            },
            "endDate": {
              "type": "string",
              "description": "End date in YYYY-MM-DD format",
              "x-zui": {
                "title": "End Date"
              }
            }
          },
          "required": ["eventTypeId"],
          "additionalProperties": false,
          "x-zui": {},
          "$schema": "http://json-schema.org/draft-07/schema#"
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "slots": {
              "type": "object",
              "additionalProperties": {
                "type": "array",
                "items": {
                  "type": "object",
                  "additionalProperties": {
                    "type": "string",
                    "x-zui": {}
                  },
                  "x-zui": {}
                },
                "x-zui": {}
              },
              "description": "Available time slots grouped by date",
              "x-zui": {
                "title": "Available Time Slots"
              }
            }
          },
          "required": ["slots"],
          "additionalProperties": false,
          "x-zui": {},
          "$schema": "http://json-schema.org/draft-07/schema#"
        }
      }
    },
    "generateLink": {
      "title": "Generate a link",
      "description": "Generates a link to a calendar",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "conversationId": {
              "type": "string",
              "description": "The ID of the conversation",
              "x-zui": {
                "placeholder": "{{ event.conversationId }}",
                "title": "Conversation ID"
              }
            },
            "email": {
              "type": "string",
              "format": "email",
              "description": "The email of the user to send the link to",
              "x-zui": {
                "title": "User Email"
              }
            },
            "eventTypeId": {
              "type": "number",
              "description": "The ID of the event type",
              "x-zui": {
                "title": "Event Type ID"
              }
            }
          },
          "required": ["conversationId", "email", "eventTypeId"],
          "additionalProperties": false,
          "x-zui": {},
          "$schema": "http://json-schema.org/draft-07/schema#"
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string",
              "description": "The generated scheduling link",
              "x-zui": {
                "title": "Scheduling Link URL"
              }
            }
          },
          "required": ["url"],
          "additionalProperties": false,
          "x-zui": {},
          "$schema": "http://json-schema.org/draft-07/schema#"
        }
      }
    },
    "bookEvent": {
      "title": "Book an Event",
      "description": "Books an event for a user",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "eventTypeId": {
              "type": "number",
              "description": "The ID of the event type",
              "x-zui": {
                "title": "Event Type ID"
              }
            },
            "start": {
              "type": "string",
              "description": "Start time in ISO 8601 format",
              "x-zui": {
                "title": "Start Time"
              }
            },
            "email": {
              "type": "string",
              "format": "email",
              "description": "The email of the user booking the event",
              "x-zui": {
                "title": "User Email"
              }
            },
            "name": {
              "type": "string",
              "description": "The name of the user booking the event",
              "x-zui": {
                "title": "User Name"
              }
            },
            "timeZone": {
              "type": "string",
              "description": "Time zone in IANA format, e.g., America/New_York",
              "x-zui": {
                "title": "Time Zone"
              }
            },
            "conversationId": {
              "type": "string",
              "description": "The ID of the conversation",
              "x-zui": {
                "placeholder": "{{ event.conversationId }}",
                "title": "Conversation ID"
              }
            }
          },
          "required": ["eventTypeId", "start", "email", "name", "timeZone"],
          "additionalProperties": false,
          "x-zui": {},
          "$schema": "http://json-schema.org/draft-07/schema#"
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "success": {
              "type": "boolean",
              "description": "Indicates if the booking was successful",
              "x-zui": {
                "title": "Success"
              }
            }
          },
          "required": ["success"],
          "additionalProperties": false,
          "x-zui": {},
          "$schema": "http://json-schema.org/draft-07/schema#"
        }
      }
    }
  },
  "interfaces": {}
}
