    {
      "location": "MODULES_ROOT/channel-telegram",
      "enabled": false
    },
    {
      "location": "MODULES_ROOT/channel-web",
      "enabled": true
    },
    {
      "location": "MODULES_ROOT/hitl",
      "enabled": false
    },
    {
      "location": "MODULES_ROOT/nlu",
      "enabled": true
    },
    {
      "location": "MODULES_ROOT/qna",
      "enabled": true
    }
