      "enabled": true
    },
    "session": {
      "enabled": false,
      "maxAge": "1h"
    }
  },
  "dialog": {
    "janitorInterval": "10s",
    "timeoutInterval": "2m",
    "sessionTimeoutInterval": "30m"
  },
  "logs": {
    "dbOutput": {
      "expiration": "2 weeks",
      "janitorInterval": "30s"
    },
    "fileOutput": {
      "enabled": false,
      "folder": "./",
      "maxFileSize": 10000
    }
  },
  "modules": [
    {
      "location": "MODULES_ROOT/analytics",
      "enabled": true
    },
    {
      "location": "MODULES_ROOT/basic-skills",
