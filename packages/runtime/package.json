{
  "name": "@botpress/runtime",
  "version": "12.26.3",
  "description": "The world's most powerful conversational engine",
  "engines": {
    "node": "^12"
  },
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "repository": "https://github.com/botpress/botpress.git",
  "author": "Botpress, Inc.",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "prebuild": "ts-node --dir ./scripts make_schemas && ts-node --dir ./scripts make_metadata",
    "build": "yarn tsc --build",
    "watch": "yarn tsc --build --watch",
    "package": "ts-node --dir ./scripts package",
    "start": "cd dist && cross-env BP_MODULES_PATH=./data/modules/:../../../modules:../../../internal-modules node index.js"
  },
  "pkg": {
    "targets": [
      "node12-win32-x64",
      "node12-linux-x64",
      "node12-macos-x64"
    ],
    "scripts": [
      "./dist/**/*.js",
      "./dist/**/*.json"
    ],
    "assets": [
      "../../node_modules/vm2/lib/*.js",
      "./node_modules/vm2/lib/*.js",
      "../../node_modules/xml-encryption/lib/templates/*.tpl.xml"
    ]
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.6",
    "@botpress/messaging-client": "1.1.5",
    "@botpress/nlu-client": "1.0.0-rc.2",
    "axios": "^0.21.1",
    "bluebird-global": "^1.0.1",
    "body-parser": "^1.18.3",
    "bytes": "^3.1.0",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "4.1.1",
    "deep-diff": "^1.0.2",
    "diff": "^4.0.1",
    "doctrine": "^2.1.0",
    "dotenv": "^7.0.0",
    "errorhandler": "^1.5.0",
    "eventemitter2": "^5.0.1",
    "express": "^4.16.4",
    "express-rate-limit": "^3.5.1",
    "fs-extra": "^9.1.0",
    "getos": "^3.1.1",
    "glob": "^7.1.6",
    "globrex": "^0.1.2",
    "inversify": "^4.13.0",
    "joi": "^13.6.0",
    "json-schema-defaults": "^0.4.0",
    "jsonlint-mod": "^1.7.5",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.1",
    "lodash": "^4.17.19",
    "lodash-decorators": "^6.0.0",
    "lru-cache": "^5.1.1",
    "mathjs": "^7.5.1",
    "mkdirp": "^1.0.4",
    "moment": "^2.24.0",
    "ms": "^2.1.1",
    "mustache": "^3.0.1",
    "nanoid": "^3.1.31",
    "nodemailer": "^6.3.0",
    "object-sizeof": "^1.5.2",
    "pg": "^7.8.0",
    "portfinder": "^1.0.20",
    "proper-lockfile": "^4.1.2",
    "querystring": "^0.2.0",
    "redlock": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "replace-in-file": "^4.1.1",
    "tar": "^4.4.19",
    "tmp": "^0.0.33",
    "unzipper": "^0.10.3",
    "verror": "^1.10.0",
    "vm2": "3.9.5",
    "yargs": "^16.0.3",
    "yn": "^2.0.0"
  },
  "resolutions": {
    "fstream": ">=1.0.12",
    "lodash": ">=4.17.21"
  },
  "devDependencies": {
    "@types/archiver": "5.1.1",
    "@types/node": "^12.18.1",
    "typescript": "4.4.4"
  },
  "optionalDependencies": {
    "ioredis": "^4.14.1",
    "sqlite3": "4.2.0"
  }
}
