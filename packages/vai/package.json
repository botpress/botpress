{
  "name": "@botpress/vai",
  "version": "0.0.5",
  "description": "Vitest AI (vai) â€“ a vitest extension for testing with LLMs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "check:type": "tsc --noEmit",
    "build": "pnpm run build:types && pnpm run build:neutral",
    "build:neutral": "ts-node -T ./build.ts",
    "build:types": "tsup",
    "watch": "tsup --watch",
    "test:e2e": "vitest run --config vitest.config.ts",
    "test:e2e:update": "vitest -u run --config vitest.config.ts",
    "test:e2e:watch": "vitest --config vitest.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "json5": "^2.2.3",
    "jsonrepair": "^3.10.0"
  },
  "devDependencies": {
    "@botpress/common": "workspace:*",
    "@types/lodash": "^4.14.191",
    "dotenv": "^16.4.4",
    "esbuild": "^0.16.12",
    "glob": "^9.3.4",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "@botpress/client": "0.43.0",
    "@bpinternal/thicktoken": "^1.0.1",
    "@bpinternal/zui": "^0.13.4",
    "lodash": "^4.17.21",
    "vitest": "^2 || ^3 || ^4 || ^5"
  }
}
