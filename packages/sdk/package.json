{
  "name": "@botpress/sdk",
  "version": "3.0.2",
  "description": "Botpress SDK",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "check:type": "tsc --noEmit",
    "build:type": "tsc --emitDeclarationOnly --declaration",
    "build:browser": "ts-node -T ./build.ts --browser",
    "build:node": "ts-node -T ./build.ts --node",
    "build": "pnpm build:type && pnpm build:node && pnpm build:browser"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@botpress/client": "0.41.2",
    "browser-or-node": "^2.1.1"
  },
  "devDependencies": {
    "esbuild": "^0.16.12",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "@bpinternal/zui": "^0.13.5",
    "esbuild": "^0.16.12"
  },
  "peerDependenciesMeta": {
    "esbuild": {
      "optional": true
    }
  }
}
