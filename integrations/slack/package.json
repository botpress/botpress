    "query-string": "^6.14.1",
    "verror": "^1.10.1"
  },
  "devDependencies": {
    "@botpress/cli": "workspace:*",
    "@botpress/client": "workspace:*",
    "@sentry/cli": "^2.18.1",
    "@types/lodash": "^4.14.191",
