{
  "name": "todoist",
  "version": "1.0.1",
  "title": "Todoist",
  "description": "Create and modify tasks, post comments and more.",
  "user": {
    "tags": {
      "id": {
        "title": "Todoist User ID",
        "description": "The unique ID of the user on Todoist"
      }
    }
  },
  "events": {
    "taskAdded": {
      "title": "Task Added",
      "description": "A task has been added",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The ID of the task",
            "x-zui": {
              "title": "ID"
            }
          },
          "content": {
            "type": "string",
            "description": "The text of the task. This value may contain markdown-formatted text and hyperlinks.",
            "x-zui": {
              "title": "Content"
            }
          },
          "description": {
            "type": "string",
            "description": "A description for the task. This value may contain markdown-formatted text and hyperlinks.",
            "x-zui": {
              "title": "Description"
            }
          },
          "priority": {
            "type": "number",
            "minimum": 1,
            "maximum": 4,
            "description": "Task priority from 1 (urgent) to 4 (lowest, default value).",
            "x-zui": {
              "title": "Priority"
            }
          }
        },
        "required": ["id", "content", "description", "priority"],
        "additionalProperties": false,
        "x-zui": {},
        "$schema": "http://json-schema.org/draft-07/schema#"
      }
    },
    "taskPriorityChanged": {
      "title": "Task Priority Changed",
      "description": "The priority of a task has been changed. The old priority is only available if the bot user is at the origin of the change",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The ID of the task",
            "x-zui": {
              "title": "ID"
            }
          },
          "newPriority": {
            "type": "number",
            "minimum": 1,
            "maximum": 4,
            "description": "Task priority from 1 (urgent) to 4 (lowest, default value).",
            "x-zui": {
              "title": "New Priority"
            }
          },
          "oldPriority": {
            "type": "number",
            "minimum": 1,
            "maximum": 4,
            "description": "Task priority from 1 (urgent) to 4 (lowest, default value).",
            "x-zui": {
              "title": "Old Priority"
            }
          }
        },
        "required": ["id", "newPriority"],
        "additionalProperties": false,
        "x-zui": {},
        "$schema": "http://json-schema.org/draft-07/schema#"
      }
    },
    "taskCompleted": {
      "title": "Task Completed",
      "description": "A task has been completed",
      "schema": {
        "type": "object",
        "properties": {
          "user_id": {
            "type": "string",
            "description": "The ID of the user who completed the task",
            "x-zui": {
              "title": "User ID"
            }
          },
          "id": {
            "type": "string",
            "description": "The ID of the task",
            "x-zui": {
              "title": "ID"
            }
          },
          "content": {
            "type": "string",
            "description": "The text of the task. This value may contain markdown-formatted text and hyperlinks.",
            "x-zui": {
              "title": "Content"
            }
          },
          "description": {
            "type": "string",
            "description": "A description for the task. This value may contain markdown-formatted text and hyperlinks.",
            "x-zui": {
              "title": "Description"
            }
          },
          "priority": {
            "type": "number",
            "minimum": 1,
            "maximum": 4,
            "description": "Task priority from 1 (urgent) to 4 (lowest, default value).",
            "x-zui": {
              "title": "Priority"
            }
          }
