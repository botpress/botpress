                  "name": {
                    "type": "string",
                    "description": "The name of the project.",
                    "x-zui": {
                      "title": "Name"
                    }
                  },
                  "color": {
                    "type": "string",
                    "enum": [
                      "berry_red",
                      "red",
                      "orange",
                      "yellow",
                      "olive_green",
                      "lime_green",
                      "green",
                      "mint_green",
                      "teal",
                      "sky_blue",
                      "light_blue",
                      "blue",
                      "grape",
                      "violet",
                      "lavender",
                      "magenta",
                      "salmon",
                      "charcoal",
                      "gunmetal",
                      "taupe"
