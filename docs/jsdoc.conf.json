{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "./_jsdoc_template",
    "encoding": "utf8",
    "destination": "./reference",
    "recurse": true,
    "readme": "../packages/core/botpress/CHANGELOG.md"
  }
}
