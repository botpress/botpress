{
  "name": "botpress",
  "description": "The world's first CMS for bots. Easily create, manage and extend chatbots.",
  "version": "0.1.3",
  "author": "Botpress",
  "babel": {
    "presets": [
      "latest",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "ignore": [
      "cli/templates/**",
      "src/web/**"
    ]
  },
  "bin": {
    "bp": "./bin/botpress",
    "botpress": "./bin/botpress"
  },
  "bugs": {
    "url": "https://github.com/botpress/botpress/issues"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "dotenv": "^4.0.0",
    "eventemitter2": "^2.1.3",
    "express": "^4.14.0",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.12.6",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "ms": "^0.7.1",
    "mware": "0.0.3",
    "node-machine-id": "^1.1.3",
    "node-sass": "^3.13.1",
    "pg": "^6.1.2",
    "prompt": "^1.0.0",
    "socket.io": "^1.5.0",
    "socketio-jwt": "^4.5.0",
    "source-map-support": "^0.4.6",
    "sqlite3": "^3.1.8",
    "universal-analytics": "^0.4.8",
    "uuid": "^3.0.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "bootstrap": "^3.3.7",
    "chai": "^3.5.0",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "hard-source-webpack-plugin": "^0.3.7",
    "history": "^2.1.2",
    "json-loader": "^0.5.4",
    "keymirror": "^0.1.1",
    "mocha": "^3.1.2",
    "ncp": "^2.0.0",
    "npm-watch": "^0.1.6",
    "nuclear-js": "^1.4.0",
    "nuclear-js-react-addons": "^0.4.1",
    "postcss-loader": "^1.1.1",
    "react": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-bootstrap-button-loader": "^1.0.7",
    "react-dom": "^15.3.1",
    "react-emoji": "^0.4.4",
    "react-fontawesome": "^1.2.0",
    "react-ga": "^2.1.2",
    "react-router": "^2.8.1",
    "react-sidebar": "^2.2.1",
    "react-sortable": "^1.2.0",
    "sass-loader": "^4.0.2",
    "script-loader": "^0.7.0",
    "simple-line-icons": "^2.4.1",
    "style-loader": "^0.13.1",
    "testcafe": "^0.13.0",
    "tmp": "0.0.31",
    "webpack": "^1.13.2",
    "webpack-node-externals": "^1.5.4"
  },
  "engines": {
    "node": ">=4.0"
  },
  "homepage": "https://github.com/botpress/botpress#readme",
  "keywords": [
    "bot",
    "bots",
    "chatbot",
    "chatbots"
  ],
  "license": "AGPL-3.0",
  "main": "index.js",
  "repository": "git+https://github.com/botpress/botpress.git",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "scripts": {
    "compile": "rm -rf lib/ && babel src/ -d lib/ -s && cp -a src/cli/templates/. lib/cli/templates && node webpack.js --compile",
    "test": "npm run compile && mocha test/unit && npm run init-test-bot && npm run e2e",
    "watch": "npm-watch",
    "init-test-bot": "rm -rf test/e2e/bot && node test/e2e/init-bot",
    "run-test-bot": "node test/e2e/start-bot.js",
    "e2e": "testcafe chrome test/e2e/tests/**/*.js -a \"npm run run-test-bot\" --app-init-delay 10000"
  },
  "watch": {
    "compile": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,scss,json,html"
    }
  }
}
