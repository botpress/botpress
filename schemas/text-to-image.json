{
  "name": "textToImage",
  "version": "2.1.0",
  "entities": {
    "imageModelRef": {
      "schema": {
        "type": "object",
        "description": "Model to use for image generation",
        "properties": {
          "id": {
            "type": "string",
            "x-zui": {}
          }
        },
        "required": [
          "id"
        ],
        "additionalProperties": false,
        "x-zui": {}
      }
    },
    "imageGenerationParams": {
      "schema": {
        "type": "object",
        "description": "Model-specific parameters for image generation",
        "properties": {},
        "additionalProperties": false,
        "x-zui": {}
      }
    }
  },
  "events": {},
  "actions": {
    "generateImage": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "model": {
              "$ref": "imageModelRef",
              "description": "Model to use for image generation",
              "x-zui": {}
            },
            "prompt": {
              "type": "string",
              "x-zui": {}
            },
            "size": {
              "type": "string",
              "x-zui": {}
            },
            "expiration": {
              "type": "integer",
              "description": "Expiration of the generated image in days, after which the image will be automatically deleted to free up storage space in your account. The default is to keep the image indefinitely (no expiration). The minimum is 30 days and the maximum is 90 days.",
              "x-zui": {},
              "minimum": 30,
              "maximum": 90
            },
            "params": {
              "$ref": "imageGenerationParams",
              "description": "Model-specific parameters for image generation",
              "x-zui": {}
            }
          },
          "required": [
            "prompt"
          ],
          "additionalProperties": false,
          "x-zui": {}
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "model": {
              "type": "string",
              "description": "Model name used",
              "x-zui": {}
            },
            "imageUrl": {
              "type": "string",
              "description": "Temporary URL of generated image",
              "x-zui": {}
            },
            "cost": {
              "type": "number",
              "description": "Cost of the image generation, in U.S. dollars (DEPRECATED)",
              "x-zui": {}
            },
            "botpress": {
              "type": "object",
              "properties": {
                "cost": {
                  "type": "number",
                  "description": "Cost of the image generation, in U.S. dollars",
                  "x-zui": {}
                }
              },
              "required": [
                "cost"
              ],
              "additionalProperties": false,
              "x-zui": {}
            }
          },
          "required": [
            "model",
            "imageUrl",
            "cost",
            "botpress"
          ],
          "additionalProperties": false,
          "x-zui": {}
        }
      }
    },
    "listImageModels": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {},
          "additionalProperties": false,
          "x-zui": {}
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "models": {
              "type": "array",
              "items": {
                "allOf": [
                  {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "x-zui": {}
                      },
                      "name": {
                        "type": "string",
                        "x-zui": {}
                      },
                      "costPerImage": {
                        "type": "number",
                        "description": "Cost per image generation, in U.S. dollars",
                        "x-zui": {}
                      },
                      "sizes": {
                        "type": "array",
                        "description": "Available image sizes",
                        "items": {
                          "type": "string",
                          "x-zui": {}
                        },
                        "x-zui": {}
                      },
                      "defaultSize": {
                        "type": "string",
                        "description": "Default image size generated by model",
                        "x-zui": {}
                      }
                    },
                    "required": [
                      "id",
                      "name",
                      "costPerImage",
                      "sizes",
                      "defaultSize"
                    ],
                    "additionalProperties": false,
                    "x-zui": {}
                  },
                  {
                    "$ref": "imageModelRef",
                    "description": "Model to use for image generation",
                    "x-zui": {}
                  }
                ],
                "x-zui": {}
              },
              "x-zui": {}
            }
          },
          "required": [
            "models"
          ],
          "additionalProperties": false,
          "x-zui": {}
        }
      }
    }
  },
  "channels": {}
}
