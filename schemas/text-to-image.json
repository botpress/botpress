{
  "actions": {
    "generateImage": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "expiration": {
              "description": "Expiration of the generated image in days, after which the image will be automatically deleted to free up storage space in your account. The default is to keep the image indefinitely (no expiration). The minimum is 30 days and the maximum is 90 days.",
              "maximum": 90,
              "minimum": 30,
              "type": "integer"
            },
            "model": { "$ref": "imageModelRef", "description": "Model to use for image generation" },
            "params": { "$ref": "imageGenerationParams" },
            "prompt": { "type": "string" },
            "size": { "type": "string" }
          },
          "required": ["prompt"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "botpress": {
              "additionalProperties": false,
              "properties": {
                "cost": { "description": "Cost of the image generation, in U.S. dollars", "type": "number" }
              },
              "required": ["cost"],
              "type": "object"
            },
            "cost": { "description": "Cost of the image generation, in U.S. dollars (DEPRECATED)", "type": "number" },
            "imageUrl": { "description": "Temporary URL of generated image", "type": "string" },
            "model": { "description": "Model name used", "type": "string" }
          },
          "required": ["model", "imageUrl", "cost", "botpress"],
          "type": "object"
        }
      }
    },
    "listImageModels": {
      "input": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "models": {
              "items": {
                "allOf": [
                  {
                    "properties": {
                      "costPerImage": { "description": "Cost per image generation, in U.S. dollars", "type": "number" },
                      "defaultSize": { "description": "Default image size generated by model", "type": "string" },
                      "id": { "type": "string" },
                      "name": { "type": "string" },
                      "sizes": {
                        "description": "Available image sizes",
                        "items": { "type": "string" },
                        "type": "array"
                      }
                    },
                    "required": ["id", "name", "costPerImage", "sizes", "defaultSize"],
                    "type": "object"
                  },
                  { "$ref": "imageModelRef" }
                ]
              },
              "type": "array"
            }
          },
          "required": ["models"],
          "type": "object"
        }
      }
    }
  },
  "channels": {},
  "entities": {
    "imageGenerationParams": {
      "schema": {
        "additionalProperties": false,
        "description": "Model-specific parameters for image generation",
        "properties": {},
        "type": "object"
      }
    },
    "imageModelRef": {
      "schema": {
        "additionalProperties": false,
        "properties": { "id": { "type": "string" } },
        "required": ["id"],
        "type": "object"
      }
    }
  },
  "events": {},
  "name": "textToImage",
  "version": "2.1.0"
}
