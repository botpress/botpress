    "appendValues": {
      "title": "Append Values",
      "description": "Appends values to the spreadsheet. The input range is used to search for existing data and find a \"table\" within that range. Values will be appended to the next row of the table, starting with the first column of the table.",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "range": {
              "type": "string",
              "x-zui": {
                "title": "Range",
                "placeholder": "'Sheet name'!A1:F8"
              }
            },
            "majorDimension": {
              "default": "ROWS",
              "type": "string",
              "enum": [
                "ROWS",
                "COLUMNS"
              ],
              "x-zui": {
                "title": "Major Dimension"
              }
            },
            "values": {
              "type": "array",
              "items": {
                "type": "array",
                "items": {
                  "type": "string",
                  "x-zui": {
                    "title": "Stringified value"
                  }
                },
                "x-zui": {
                  "title": "Row or column"
                }
              },
              "x-zui": {
                "title": "Values"
              }
            }
          },
          "required": [
            "range",
            "values"
          ]
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "spreadsheetId": {
              "type": "string",
              "x-zui": {
                "title": "Spreadsheet ID"
              }
            }
          },
          "required": [
            "spreadsheetId"
          ]
        }
      }
