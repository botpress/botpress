      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "range": {
              "type": "string",
              "description": "The range the values cover, in A1 notation. This range indicates the entire requested range, even though the values will exclude trailing rows and columns. (e.g. \"Sheet1!A1:B2\")",
              "x-zui": {
                "title": "Range",
                "placeholder": "'Sheet name'!A1:F8"
              }
            },
            "majorDimension": {
              "type": "string",
              "enum": ["ROWS", "COLUMNS"],
              "x-zui": {
                "title": "Major Dimension"
              },
              "default": "ROWS",
              "description": "If it equals \"ROWS\", then the values are returned in rows. If it equals \"COLUMNS\", then the values are returned in columns."
            },
            "values": {
              "type": "array",
              "items": {
                "type": "array",
                "items": {
                  "type": "string",
                  "description": "Represents the value of a single cell. This is a stringified number, string or boolean value",
                  "x-zui": {
                    "title": "Stringified value"
                  }
                },
                "description": "Represents a major dimension (a row or column) of a values range",
                "x-zui": {
                  "title": "Row or column"
                }
              },
              "description": "The data that was read. This is an array of arrays, the outer array representing all the data and each inner array representing a major dimension (a row or column). Each item in the inner array corresponds with one cell. (e.g. [[\"a\", \"b\"], [\"c\", \"d\"]])",
              "x-zui": {
                "title": "Values"
              }
            }
          },
          "required": ["range", "values"],
          "additionalProperties": false
        }
