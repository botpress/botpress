{
  "name": "gsheets",
  "version": "1.1.2",
  "title": "Google Sheets",
  "description": "Access, update, and append Google Sheets data.",
  "user": {},
  "events": {},
  "actions": {
    "addSheet": {
      "title": "Add Sheet",
      "description": "Adds a new sheet to the spreadsheet.",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "description": "The title of the new sheet to add to the spreadsheet.",
              "x-zui": {
                "title": "Title"
              }
            }
          },
          "required": ["title"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "spreadsheetId": {
              "type": "string",
              "description": "The spreadsheet ID of the new sheet.",
              "x-zui": {
                "title": "Spreadsheet ID"
              }
            },
            "newSheet": {
              "type": "object",
              "properties": {
                "sheetId": {
                  "type": "number",
                  "description": "The ID of the new sheet.",
                  "x-zui": {
                    "title": "Sheet ID"
                  }
                },
                "title": {
                  "type": "string",
                  "description": "The title of the new sheet.",
                  "x-zui": {
                    "title": "Title"
                  }
                },
                "index": {
                  "type": "number",
                  "description": "The index of the new sheet within the spreadsheet.",
                  "x-zui": {
                    "title": "Index"
                  }
                },
                "isHidden": {
                  "type": "boolean",
                  "description": "Whether the new sheet is hidden.",
                  "x-zui": {
                    "title": "Is Hidden"
                  }
                }
              },
              "required": ["sheetId", "title", "index", "isHidden"],
              "additionalProperties": false,
              "description": "The new sheet that was added to the spreadsheet.",
              "x-zui": {
                "title": "New Sheet"
              }
            }
          },
          "required": ["spreadsheetId", "newSheet"],
          "additionalProperties": false
        }
      }
    },
    "appendValues": {
      "title": "Append Values",
      "description": "Appends values to the spreadsheet. The input range is used to search for existing data and find a \"table\" within that range. Values will be appended to the next row of the table, starting with the first column of the table.",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "range": {
              "type": "string",
              "description": "The A1 notation of a range to search for a logical table of data. Values are appended after the last row of the table. (e.g. \"Sheet1!A1:B2\")",
              "x-zui": {
                "title": "Range",
                "placeholder": "'Sheet name'!A1:F8"
              }
            },
            "majorDimension": {
              "type": "string",
              "enum": ["ROWS", "COLUMNS"],
              "x-zui": {
                "title": "Major Dimension"
              },
              "default": "ROWS",
              "description": "If it equals \"ROWS\", then the values are inserted as rows. If it equals \"COLUMNS\", then the values are inserted as columns."
            },
            "values": {
              "type": "array",
              "items": {
                "type": "array",
                "items": {
                  "type": "string",
                  "description": "Represents the value of a single cell. This is a stringified number, string or boolean value",
                  "x-zui": {
                    "title": "Stringified value"
                  }
                },
                "description": "Represents a major dimension (a row or column) of a values range",
                "x-zui": {
                  "title": "Row or column"
                }
              },
              "description": "The values to write to the range. This is an array of arrays, where each inner array represents a major dimension (a row or column) of data. (e.g. [[\"a\", \"b\"], [\"c\", \"d\"]])",
              "x-zui": {
                "title": "Values"
              }
            }
          },
          "required": ["range", "values"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "spreadsheetId": {
              "type": "string",
              "description": "The spreadsheet the updates were applied to.",
              "x-zui": {
                "title": "Spreadsheet ID"
              }
            }
          },
          "required": ["spreadsheetId"],
          "additionalProperties": false
        }
      }
    },
    "clearValues": {
      "title": "Clear Values",
      "description": "Clears values from a spreadsheet. Only values are cleared; all other properties of the cell (such as formatting, data validation, etc..) are kept.",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "range": {
              "type": "string",
              "description": "The A1 notation of the range to clear. (e.g. \"Sheet1!A1:B2\")",
              "x-zui": {
                "title": "Range",
                "placeholder": "'Sheet name'!A1:F8"
              }
            }
          },
          "required": ["range"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "spreadsheetId": {
              "type": "string",
              "description": "The spreadsheet the updates were applied to.",
              "x-zui": {
                "title": "Spreadsheet ID"
              }
            },
            "clearedRange": {
              "type": "string",
              "description": "The range (in A1 notation) that was cleared.",
              "x-zui": {
                "title": "Cleared Range"
              }
            }
          },
          "required": ["spreadsheetId", "clearedRange"],
          "additionalProperties": false
        }
      }
    },
