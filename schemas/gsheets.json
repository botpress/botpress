    "getValues": {
      "title": "Get Values",
      "description": "Returns the values of a range in the spreadsheet.",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "range": {
              "type": "string",
              "description": "The A1 notation of the range to retrieve. (e.g. \"Sheet1!A1:B2\")",
