{
  "actions": {
    "generateContent": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "debug": { "type": "boolean" },
            "maxTokens": { "type": "number" },
            "messages": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "content": {
                    "anyOf": [
                      {
                        "anyOf": [
                          { "type": "string" },
                          {
                            "items": {
                              "additionalProperties": false,
                              "properties": {
                                "mimeType": { "type": "string" },
                                "text": { "type": "string" },
                                "type": { "enum": ["text", "image"], "type": "string" },
                                "url": { "type": "string" }
                              },
                              "required": ["type"],
                              "type": "object"
                            },
                            "type": "array"
                          }
                        ]
                      },
                      { "type": "null" }
                    ]
                  },
                  "role": { "enum": ["user", "assistant"], "type": "string" },
                  "toolCalls": {
                    "items": {
                      "additionalProperties": false,
                      "properties": {
                        "function": {
                          "additionalProperties": false,
                          "properties": {
                            "arguments": {
                              "anyOf": [{ "additionalProperties": {}, "type": "object" }, { "type": "null" }]
                            },
                            "name": { "type": "string" }
                          },
                          "required": ["name", "arguments"],
                          "type": "object"
                        },
                        "id": { "type": "string" },
                        "type": { "const": "function", "type": "string" }
                      },
                      "required": ["id", "type", "function"],
                      "type": "object"
                    },
                    "type": "array"
                  },
                  "toolResultCallId": { "type": "string" },
                  "type": {
                    "default": "text",
                    "enum": ["text", "tool_calls", "tool_result", "multipart"],
                    "type": "string"
                  }
                },
                "required": ["role", "content"],
                "type": "object"
              },
              "type": "array"
            },
            "meta": {
              "additionalProperties": false,
              "properties": {
                "integrationName": { "type": "string" },
                "promptCategory": { "type": "string" },
                "promptSource": { "type": "string" }
              },
              "type": "object"
            },
            "model": {
              "additionalProperties": false,
              "properties": {
                "id": {
                  "description": "Model to use for content generation",
                  "enum": [
                    "deepseek-r1-distill-llama-70b",
                    "llama-3.3-70b-versatile",
                    "llama-3.2-1b-preview",
                    "llama-3.2-3b-preview",
                    "llama-3.2-11b-vision-preview",
                    "llama-3.2-90b-vision-preview",
                    "llama-3.1-8b-instant",
                    "llama3-8b-8192",
                    "llama3-70b-8192",
                    "gemma2-9b-it"
                  ],
                  "type": "string",
                  "x-zui": { "placeholder": "mixtral-8x7b-32768" }
                }
              },
              "required": ["id"],
              "type": "object"
            },
            "reasoningEffort": { "enum": ["low", "medium", "high"], "type": "string" },
            "responseFormat": { "enum": ["text", "json_object"], "type": "string" },
            "stopSequences": { "items": { "type": "string" }, "type": "array" },
            "systemPrompt": { "type": "string" },
            "temperature": { "default": 1, "type": "number" },
            "toolChoice": {
              "additionalProperties": false,
              "properties": {
                "functionName": { "type": "string" },
                "type": { "enum": ["auto", "specific", "any", "none", ""], "type": "string" }
              },
              "type": "object"
            },
            "tools": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "function": {
                    "additionalProperties": false,
                    "properties": {
                      "argumentsSchema": { "additionalProperties": false, "properties": {}, "type": "object" },
                      "description": { "type": "string" },
                      "name": { "type": "string" }
                    },
                    "required": ["name"],
                    "type": "object"
                  },
                  "type": { "const": "function", "type": "string" }
                },
                "required": ["type", "function"],
                "type": "object"
              },
              "type": "array"
            },
            "topP": { "default": 1, "type": "number" },
            "userId": { "type": "string" }
          },
          "required": ["messages"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "botpress": {
              "additionalProperties": false,
              "properties": { "cost": { "type": "number" } },
              "required": ["cost"],
              "type": "object"
            },
            "choices": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "content": {
                    "anyOf": [
                      {
                        "anyOf": [
                          { "type": "string" },
                          {
                            "items": {
                              "additionalProperties": false,
                              "properties": {
                                "mimeType": { "type": "string" },
                                "text": { "type": "string" },
                                "type": { "enum": ["text", "image"], "type": "string" },
                                "url": { "type": "string" }
                              },
                              "required": ["type"],
                              "type": "object"
                            },
                            "type": "array"
                          }
                        ]
                      },
                      { "type": "null" }
                    ]
                  },
                  "index": { "type": "number" },
                  "role": { "const": "assistant", "type": "string" },
                  "stopReason": {
                    "enum": ["stop", "max_tokens", "tool_calls", "content_filter", "other"],
                    "type": "string"
                  },
                  "toolCalls": {
                    "items": {
                      "additionalProperties": false,
                      "properties": {
                        "function": {
                          "additionalProperties": false,
                          "properties": {
                            "arguments": {
                              "anyOf": [{ "additionalProperties": {}, "type": "object" }, { "type": "null" }]
                            },
                            "name": { "type": "string" }
                          },
                          "required": ["name", "arguments"],
                          "type": "object"
                        },
                        "id": { "type": "string" },
                        "type": { "const": "function", "type": "string" }
                      },
                      "required": ["id", "type", "function"],
                      "type": "object"
                    },
                    "type": "array"
                  },
                  "toolResultCallId": { "type": "string" },
                  "type": {
                    "default": "text",
                    "enum": ["text", "tool_calls", "tool_result", "multipart"],
                    "type": "string"
                  }
                },
                "required": ["content", "role", "index", "stopReason"],
                "type": "object"
              },
              "type": "array"
            },
            "id": { "type": "string" },
            "model": { "type": "string" },
            "provider": { "type": "string" },
            "usage": {
              "additionalProperties": false,
              "properties": {
                "inputCost": { "type": "number" },
                "inputTokens": { "type": "number" },
                "outputCost": { "type": "number" },
                "outputTokens": { "type": "number" }
              },
              "required": ["inputTokens", "inputCost", "outputTokens", "outputCost"],
              "type": "object"
            }
          },
          "required": ["id", "provider", "model", "choices", "usage", "botpress"],
          "type": "object"
        }
      }
    },
    "listLanguageModels": {
      "input": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "models": {
              "items": {
                "allOf": [
                  {
                    "properties": {
                      "description": { "type": "string" },
                      "id": { "type": "string" },
                      "input": {
                        "additionalProperties": false,
                        "properties": { "costPer1MTokens": { "type": "number" }, "maxTokens": { "type": "number" } },
                        "required": ["maxTokens", "costPer1MTokens"],
                        "type": "object"
                      },
                      "name": { "type": "string" },
                      "output": {
                        "additionalProperties": false,
                        "properties": { "costPer1MTokens": { "type": "number" }, "maxTokens": { "type": "number" } },
                        "required": ["maxTokens", "costPer1MTokens"],
                        "type": "object"
                      },
                      "tags": {
                        "items": {
                          "enum": [
                            "recommended",
                            "deprecated",
                            "general-purpose",
                            "low-cost",
                            "vision",
                            "coding",
                            "agents",
                            "function-calling",
                            "roleplay",
                            "storytelling",
                            "reasoning",
                            "preview"
                          ],
                          "type": "string"
                        },
                        "type": "array"
                      }
                    },
                    "required": ["id", "name", "description", "tags", "input", "output"],
                    "type": "object"
                  },
                  {
                    "properties": {
                      "id": {
                        "description": "Model to use for content generation",
                        "enum": [
                          "deepseek-r1-distill-llama-70b",
                          "llama-3.3-70b-versatile",
                          "llama-3.2-1b-preview",
                          "llama-3.2-3b-preview",
                          "llama-3.2-11b-vision-preview",
                          "llama-3.2-90b-vision-preview",
                          "llama-3.1-8b-instant",
                          "llama3-8b-8192",
                          "llama3-70b-8192",
                          "gemma2-9b-it"
                        ],
                        "type": "string",
                        "x-zui": { "placeholder": "mixtral-8x7b-32768" }
                      }
                    },
                    "required": ["id"],
                    "type": "object"
                  }
                ]
              },
              "type": "array"
            }
          },
          "required": ["models"],
          "type": "object"
        }
      }
    },
    "listSpeechToTextModels": {
      "input": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "models": {
              "items": {
                "allOf": [
                  {
                    "properties": {
                      "costPerMinute": { "type": "number" },
                      "id": { "type": "string" },
                      "name": { "type": "string" }
                    },
                    "required": ["id", "name", "costPerMinute"],
                    "type": "object"
                  },
                  { "properties": { "id": { "type": "string" } }, "required": ["id"], "type": "object" }
                ]
              },
              "type": "array"
            }
          },
          "required": ["models"],
          "type": "object"
        }
      }
    },
    "transcribeAudio": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "fileUrl": { "type": "string" },
            "language": { "type": "string" },
            "model": {
              "additionalProperties": false,
              "properties": { "id": { "type": "string" } },
              "required": ["id"],
              "type": "object"
            },
            "prompt": { "type": "string" },
            "temperature": { "default": 0, "type": "number" }
          },
          "required": ["fileUrl"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "botpress": {
              "additionalProperties": false,
              "properties": { "cost": { "type": "number" } },
              "required": ["cost"],
              "type": "object"
            },
            "cost": { "type": "number" },
            "duration": { "type": "number" },
            "language": { "type": "string" },
            "model": { "type": "string" },
            "segments": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "avg_logprob": { "type": "number" },
                  "compression_ratio": { "type": "number" },
                  "end": { "type": "number" },
                  "id": { "type": "number" },
                  "no_speech_prob": { "type": "number" },
                  "seek": { "type": "number" },
                  "start": { "type": "number" },
                  "temperature": { "type": "number" },
                  "text": { "type": "string" },
                  "tokens": { "items": { "type": "number" }, "type": "array" }
                },
                "required": [
                  "text",
                  "id",
                  "seek",
                  "start",
                  "end",
                  "tokens",
                  "temperature",
                  "avg_logprob",
                  "compression_ratio",
                  "no_speech_prob"
                ],
                "type": "object"
              },
              "type": "array"
            }
          },
          "required": ["language", "duration", "segments", "model", "cost", "botpress"],
          "type": "object"
        }
      }
    }
  },
  "channels": {},
  "description": "Gain access to Groq models for content generation, chat responses, and audio transcription.",
  "entities": {
    "modelRef": {
      "schema": {
        "additionalProperties": false,
        "properties": {
          "id": {
            "description": "Model to use for content generation",
            "enum": [
              "deepseek-r1-distill-llama-70b",
              "llama-3.3-70b-versatile",
              "llama-3.2-1b-preview",
              "llama-3.2-3b-preview",
              "llama-3.2-11b-vision-preview",
              "llama-3.2-90b-vision-preview",
              "llama-3.1-8b-instant",
              "llama3-8b-8192",
              "llama3-70b-8192",
              "gemma2-9b-it"
            ],
            "type": "string",
            "x-zui": { "placeholder": "mixtral-8x7b-32768" }
          }
        },
        "required": ["id"],
        "type": "object"
      }
    },
    "speechToTextModelRef": {
      "schema": {
        "additionalProperties": false,
        "properties": { "id": { "type": "string" } },
        "required": ["id"],
        "type": "object"
      }
    }
  },
  "events": {},
  "interfaces": {
    "llm<modelRef>": {
      "actions": {
        "generateContent": { "name": "generateContent" },
        "listLanguageModels": { "name": "listLanguageModels" }
      },
      "channels": {},
      "entities": { "modelRef": { "name": "modelRef" } },
      "events": {},
      "id": "",
      "name": "llm",
      "version": "8.0.0"
    },
    "speechToText<speechToTextModelRef>": {
      "actions": {
        "listSpeechToTextModels": { "name": "listSpeechToTextModels" },
        "transcribeAudio": { "name": "transcribeAudio" }
      },
      "channels": {},
      "entities": { "speechToTextModelRef": { "name": "speechToTextModelRef" } },
      "events": {},
      "id": "",
      "name": "speechToText",
      "version": "2.0.0"
    }
  },
  "name": "groq",
  "title": "Groq",
  "version": "13.0.0"
}
