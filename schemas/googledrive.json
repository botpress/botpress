          "name": {
            "type": "string",
            "minLength": 1
          },
          "parentId": {
            "type": "string",
            "minLength": 1
          },
          "mimeType": {
            "type": "string",
            "const": "application/vnd.google-apps.folder"
          },
          "path": {
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1
            }
          }
        },
        "required": [
          "id",
          "name",
          "mimeType",
          "path"
        ]
      }
    },
    "folderDeleted": {
      "title": "Folder Deleted",
