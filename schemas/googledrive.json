{
  "name": "googledrive",
  "version": "0.0.4",
  "title": "Google Drive",
  "description": "Access and manage your Google Drive files from your bot.",
  "events": {
    "fileCreated": {
      "title": "File Created",
      "description": "Triggered when a file is created in Google Drive",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "minLength": 1
          },
          "name": {
            "type": "string",
            "minLength": 1
          },
          "parentId": {
            "type": "string",
            "minLength": 1
          },
          "mimeType": {
            "type": "string",
            "minLength": 1
          },
          "size": {
            "type": "number",
            "minimum": 0
          },
          "path": {
            "type": "array",
            "items": {
              "type": "string",
              "minLength": 1
            }
          }
        },
        "required": [
          "id",
          "name",
          "mimeType",
          "size",
          "path"
        ]
      }
    },
    "fileDeleted": {
      "title": "File Deleted",
      "description": "Triggered when a file is deleted in Google Drive",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": [
          "id"
        ]
      }
    },
    "folderCreated": {
      "title": "Folder Created",
      "description": "Triggered when a folder is created in Google Drive",
