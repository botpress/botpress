        "properties": {
          "filesCache": {
            "type": "object",
            "additionalProperties": {
              "anyOf": [
                {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string",
                      "minLength": 1
                    },
                    "name": {
                      "type": "string",
                      "minLength": 1
                    },
                    "parentId": {
                      "type": "string",
                      "minLength": 1
                    },
                    "mimeType": {
