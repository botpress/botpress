                    },
                    "size": {
                      "type": "number",
                      "minimum": 0
                    },
                    "type": {
                      "type": "string",
                      "const": "normal"
                    }
                  },
                  "required": [
                    "id",
                    "name",
                    "mimeType",
                    "size",
                    "type"
                  ]
                },
                {
