{
  "name": "listable",
  "version": "0.0.1",
  "entities": {
    "item": {
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      }
    }
  },
  "events": {},
  "actions": {
    "list": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "nextToken": {
              "type": "string"
            }
          },
          "required": []
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "allOf": [
                  {
                    "$ref": "item"
                  },
                  {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "id"
                    ]
                  }
                ]
              }
            },
            "meta": {
              "type": "object",
              "properties": {
                "nextToken": {
                  "type": "string"
                }
              },
              "required": []
            }
          },
          "required": [
            "items",
            "meta"
          ]
        }
      }
    }
  },
  "channels": {}
}
