{
  "actions": {
    "createTask": {
      "description": "Create a new task in a list",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "assignees": {
              "description": "IDs of the assignees of the task to be created",
              "items": { "type": "number" },
              "type": "array",
              "x-zui": { "title": "Assignees" }
            },
            "description": {
              "description": "Description of the task to be created",
              "type": "string",
              "x-zui": { "title": "Description" }
            },
            "dueDate": {
              "description": "Due date of the task to be created",
              "format": "date-time",
              "type": "string",
              "x-zui": { "title": "Due date" }
            },
            "listId": {
              "description": "ID of the list to create the task in",
              "type": "string",
              "x-zui": { "title": "List ID" }
            },
            "name": { "description": "Name of the task to be created", "type": "string", "x-zui": { "title": "Name" } },
            "status": {
              "description": "Status of the task to be created",
              "type": "string",
              "x-zui": { "title": "Status" }
            },
            "tags": {
              "description": "Tags of the task to be created",
              "items": { "type": "string" },
              "type": "array",
              "x-zui": { "title": "Tags" }
            }
          },
          "required": ["listId", "name"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "taskId": { "description": "Id of the task created", "type": "string", "x-zui": { "title": "Task ID" } }
          },
          "required": ["taskId"],
          "type": "object"
        }
      },
      "title": "Create a task"
    },
    "deleteTask": {
      "description": "Delete a task",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "taskId": {
              "description": "ID of the task to be deleted",
              "type": "string",
              "x-zui": { "title": "Task ID" }
            }
          },
          "required": ["taskId"],
          "type": "object"
        }
      },
      "output": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } },
      "title": "Delete a task"
    },
    "getListMembers": {
      "description": "Get all the members of a list",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "listId": {
              "description": "ID of the list to get the members of",
              "type": "string",
              "x-zui": { "title": "List ID" }
            }
          },
          "required": ["listId"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "members": {
              "description": "Members of the list",
              "items": {
                "additionalProperties": false,
                "properties": {
                  "email": { "description": "Email of the member", "type": "string", "x-zui": { "title": "Email" } },
                  "id": { "description": "ID of the member", "type": "number", "x-zui": { "title": "Member ID" } },
                  "username": {
                    "description": "Username of the member",
                    "type": "string",
                    "x-zui": { "title": "Username" }
                  }
                },
                "required": ["id", "username", "email"],
                "type": "object"
              },
              "type": "array",
              "x-zui": { "title": "Members" }
            }
          },
          "required": ["members"],
          "type": "object"
        }
      },
      "title": "Get members of a list"
    },
    "updateTask": {
      "description": "Update the details of a task",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "archived": {
              "description": "New archived status of the task",
              "type": "boolean",
              "x-zui": { "title": "Archived" }
            },
            "assigneesToAdd": {
              "description": "Members to add to the task",
              "items": { "type": "number" },
              "type": "array",
              "x-zui": { "title": "Assignees to add" }
            },
            "assigneesToRemove": {
              "description": "Members to remove from the task",
              "items": { "type": "number" },
              "type": "array",
              "x-zui": { "title": "Assignees to remove" }
            },
            "description": {
              "description": "New description of the task",
              "type": "string",
              "x-zui": { "title": "Description" }
            },
            "dueDate": {
              "description": "New due date of the task",
              "format": "date-time",
              "type": "string",
              "x-zui": { "title": "Due date" }
            },
            "name": { "description": "New name of the task", "type": "string", "x-zui": { "title": "Name" } },
            "status": { "description": "New status of the task", "type": "string", "x-zui": { "title": "Status" } },
            "taskId": {
              "description": "ID of the task to be updated",
              "type": "string",
              "x-zui": { "title": "Task ID" }
            }
          },
          "required": ["taskId"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "taskId": { "description": "Id of the task updated", "type": "string", "x-zui": { "title": "Task ID" } }
          },
          "required": ["taskId"],
          "type": "object"
        }
      },
      "title": "Update a task"
    }
  },
  "channels": {
    "comment": {
      "conversation": { "tags": { "taskId": { "description": "Task ID from ClickUp", "title": "Task ID" } } },
      "description": "Comments on a task",
      "message": { "tags": { "id": { "description": "Message ID from ClickUp", "title": "Message ID" } } },
      "messages": {
        "text": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "text": { "description": "Content of the comment", "type": "string", "x-zui": { "title": "Text" } }
            },
            "required": ["text"],
            "type": "object"
          }
        }
      },
      "title": "Comment"
    }
  },
  "description": "Create and update tasks, and add comments from your chatbot.",
  "events": {
    "taskCreated": {
      "description": "Triggered when a task is created",
      "schema": {
        "additionalProperties": false,
        "properties": {
          "id": { "description": "ID of the created task", "type": "string", "x-zui": { "title": "Task ID" } }
        },
        "required": ["id"],
        "type": "object"
      },
      "title": "Task Created"
    },
    "taskDeleted": {
      "description": "Triggered when a task is deleted",
      "schema": {
        "additionalProperties": false,
        "properties": {
          "id": { "description": "ID of the deleted task", "type": "string", "x-zui": { "title": "Task ID" } }
        },
        "required": ["id"],
        "type": "object"
      },
      "title": "Task Deleted"
    },
    "taskUpdated": {
      "description": "Triggered when a task is updated",
      "schema": {
        "additionalProperties": false,
        "properties": {
          "id": { "description": "ID of the updated task", "type": "string", "x-zui": { "title": "Task ID" } }
        },
        "required": ["id"],
        "type": "object"
      },
      "title": "Task Updated"
    }
  },
  "interfaces": {},
  "name": "clickup",
  "title": "ClickUp",
  "user": { "tags": { "id": { "description": "User ID from ClickUp", "title": "User ID" } } },
  "version": "0.0.1"
}
