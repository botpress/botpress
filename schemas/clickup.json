{
  "name": "clickup",
  "version": "0.0.1",
  "title": "ClickUp",
  "description": "Create and update tasks, and add comments from your chatbot.",
  "user": {
    "tags": {
      "id": {
        "title": "User ID",
        "description": "User ID from ClickUp"
      }
    }
  },
  "events": {
    "taskCreated": {
      "title": "Task Created",
      "description": "Triggered when a task is created",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "ID of the created task",
            "x-zui": {
              "title": "Task ID"
            }
          }
        },
        "required": ["id"],
        "additionalProperties": false
      }
    },
    "taskUpdated": {
      "title": "Task Updated",
      "description": "Triggered when a task is updated",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "ID of the updated task",
            "x-zui": {
              "title": "Task ID"
            }
          }
        },
        "required": ["id"],
        "additionalProperties": false
      }
    },
    "taskDeleted": {
      "title": "Task Deleted",
      "description": "Triggered when a task is deleted",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "ID of the deleted task",
            "x-zui": {
              "title": "Task ID"
            }
          }
        },
        "required": ["id"],
        "additionalProperties": false
      }
    }
  },
  "actions": {
    "createTask": {
      "title": "Create a task",
      "description": "Create a new task in a list",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "listId": {
              "type": "string",
              "description": "ID of the list to create the task in",
              "x-zui": {
                "title": "List ID"
              }
            },
            "name": {
              "type": "string",
              "description": "Name of the task to be created",
              "x-zui": {
                "title": "Name"
              }
            },
            "description": {
              "type": "string",
              "x-zui": {
                "title": "Description"
              },
              "description": "Description of the task to be created"
            },
            "status": {
              "type": "string",
              "x-zui": {
                "title": "Status"
              },
              "description": "Status of the task to be created"
            },
            "assignees": {
              "type": "array",
              "items": {
                "type": "number"
              },
              "x-zui": {
                "title": "Assignees"
              },
              "description": "IDs of the assignees of the task to be created"
            },
            "dueDate": {
              "type": "string",
              "format": "date-time",
              "x-zui": {
                "title": "Due date"
              },
              "description": "Due date of the task to be created"
            },
            "tags": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "x-zui": {
                "title": "Tags"
              },
              "description": "Tags of the task to be created"
            }
          },
          "required": ["listId", "name"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "taskId": {
              "type": "string",
              "description": "Id of the task created",
              "x-zui": {
                "title": "Task ID"
              }
            }
          },
          "required": ["taskId"],
          "additionalProperties": false
        }
      }
    },
    "updateTask": {
      "title": "Update a task",
      "description": "Update the details of a task",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "taskId": {
              "type": "string",
              "description": "ID of the task to be updated",
              "x-zui": {
                "title": "Task ID"
              }
            },
            "name": {
              "type": "string",
              "x-zui": {
                "title": "Name"
              },
              "description": "New name of the task"
            },
            "description": {
              "type": "string",
              "x-zui": {
                "title": "Description"
              },
              "description": "New description of the task"
            },
            "status": {
              "type": "string",
              "x-zui": {
                "title": "Status"
              },
              "description": "New status of the task"
            },
            "archived": {
              "type": "boolean",
              "x-zui": {
                "title": "Archived"
              },
              "description": "New archived status of the task"
            },
            "assigneesToAdd": {
              "type": "array",
              "items": {
                "type": "number"
              },
              "x-zui": {
                "title": "Assignees to add"
              },
              "description": "Members to add to the task"
            },
            "assigneesToRemove": {
              "type": "array",
              "items": {
                "type": "number"
              },
              "x-zui": {
                "title": "Assignees to remove"
              },
              "description": "Members to remove from the task"
            },
            "dueDate": {
              "type": "string",
              "format": "date-time",
              "x-zui": {
                "title": "Due date"
              },
              "description": "New due date of the task"
            }
          },
          "required": ["taskId"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "taskId": {
              "type": "string",
              "description": "Id of the task updated",
              "x-zui": {
                "title": "Task ID"
              }
            }
          },
          "required": ["taskId"],
          "additionalProperties": false
        }
      }
    },
    "deleteTask": {
      "title": "Delete a task",
      "description": "Delete a task",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "taskId": {
              "type": "string",
              "description": "ID of the task to be deleted",
              "x-zui": {
                "title": "Task ID"
              }
            }
          },
          "required": ["taskId"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {},
          "additionalProperties": false
        }
      }
    },
    "getListMembers": {
      "title": "Get members of a list",
      "description": "Get all the members of a list",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "listId": {
              "type": "string",
              "description": "ID of the list to get the members of",
              "x-zui": {
                "title": "List ID"
              }
            }
          },
          "required": ["listId"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "members": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "number",
                    "description": "ID of the member",
                    "x-zui": {
                      "title": "Member ID"
                    }
                  },
                  "username": {
                    "type": "string",
                    "description": "Username of the member",
                    "x-zui": {
                      "title": "Username"
                    }
                  },
                  "email": {
                    "type": "string",
                    "description": "Email of the member",
                    "x-zui": {
                      "title": "Email"
                    }
                  }
                },
                "required": ["id", "username", "email"],
                "additionalProperties": false
              },
              "description": "Members of the list",
              "x-zui": {
                "title": "Members"
              }
            }
          },
          "required": ["members"],
          "additionalProperties": false
        }
      }
    }
  },
  "channels": {
    "comment": {
      "title": "Comment",
      "description": "Comments on a task",
      "messages": {
        "text": {
          "schema": {
            "type": "object",
            "properties": {
              "text": {
                "type": "string",
                "description": "Content of the comment",
                "x-zui": {
                  "title": "Text"
                }
              }
            },
            "required": ["text"],
            "additionalProperties": false
          }
        }
      },
      "message": {
        "tags": {
          "id": {
            "title": "Message ID",
            "description": "Message ID from ClickUp"
          }
        }
      },
      "conversation": {
        "tags": {
          "taskId": {
            "title": "Task ID",
            "description": "Task ID from ClickUp"
          }
        }
      }
    }
  },
  "interfaces": {}
}
