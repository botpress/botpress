{
  "name": "cerebras",
  "version": "6.0.0",
  "title": "Cerebras",
  "description": "Get access to a curated list of Cerebras models for content generation and chat completions within your bot.",
  "events": {},
  "actions": {
    "generateContent": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "model": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "enum": ["llama3.1-8b", "llama3.3-70b"],
                  "description": "Model to use for content generation",
                  "x-zui": {
                    "placeholder": "llama3.1-8b"
                  }
                }
              },
              "required": ["id"],
              "additionalProperties": false
            },
            "reasoningEffort": {
              "type": "string",
              "enum": ["low", "medium", "high"]
            },
            "systemPrompt": {
              "type": "string"
            },
            "messages": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "role": {
                    "type": "string",
                    "enum": ["user", "assistant"]
                  },
                  "type": {
                    "type": "string",
                    "enum": ["text", "tool_calls", "tool_result", "multipart"],

                    "default": "text"
                  },
                  "toolCalls": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string"
                        },
                        "type": {
                          "type": "string",
                          "const": "function"
                        },
                        "function": {
                          "type": "object",
                          "properties": {
                            "name": {
                              "type": "string"
                            },
                            "arguments": {
                              "anyOf": [
                                {
                                  "type": "object",
                                  "additionalProperties": {}
                                },
                                {
                                  "type": "null"
                                }
                              ]
                            }
                          },
                          "required": ["name", "arguments"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["id", "type", "function"],
                      "additionalProperties": false
                    }
                  },
                  "toolResultCallId": {
                    "type": "string"
                  },
                  "content": {
                    "anyOf": [
                      {
                        "anyOf": [
                          {
                            "type": "string"
                          },
                          {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "type": {
                                  "type": "string",
                                  "enum": ["text", "image"]
                                },
                                "mimeType": {
                                  "type": "string"
                                },
                                "text": {
                                  "type": "string"
                                },
                                "url": {
                                  "type": "string"
                                }
                              },
                              "required": ["type"],
                              "additionalProperties": false
                            }
                          }
                        ]
                      },
                      {
                        "type": "null"
                      }
                    ]
                  }
                },
                "required": ["role", "content"],
                "additionalProperties": false
              }
            },
            "responseFormat": {
              "type": "string",
              "enum": ["text", "json_object"]
            },
            "maxTokens": {
              "type": "number"
            },
            "temperature": {
              "type": "number",

              "default": 1
            },
            "topP": {
              "type": "number",

              "default": 1
            },
            "stopSequences": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "tools": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "const": "function"
                  },
                  "function": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      },
                      "argumentsSchema": {
                        "type": "object",
                        "properties": {},
                        "additionalProperties": false
                      }
                    },
                    "required": ["name"],
                    "additionalProperties": false
                  }
                },
                "required": ["type", "function"],
                "additionalProperties": false
              }
            },
            "toolChoice": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "enum": ["auto", "specific", "any", "none", ""]
                },
                "functionName": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "userId": {
              "type": "string"
            },
            "debug": {
              "type": "boolean"
            },
            "meta": {
              "type": "object",
              "properties": {
                "promptSource": {
                  "type": "string"
                },
                "promptCategory": {
                  "type": "string"
                },
                "integrationName": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            }
          },
          "required": ["messages"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "provider": {
              "type": "string"
            },
            "model": {
              "type": "string"
            },
            "choices": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": ["text", "tool_calls", "tool_result", "multipart"],

                    "default": "text"
                  },
                  "toolCalls": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string"
                        },
                        "type": {
                          "type": "string",
                          "const": "function"
                        },
                        "function": {
                          "type": "object",
                          "properties": {
                            "name": {
                              "type": "string"
                            },
                            "arguments": {
                              "anyOf": [
                                {
                                  "type": "object",
                                  "additionalProperties": {}
                                },
                                {
                                  "type": "null"
                                }
                              ]
                            }
                          },
                          "required": ["name", "arguments"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["id", "type", "function"],
                      "additionalProperties": false
                    }
                  },
                  "toolResultCallId": {
                    "type": "string"
                  },
                  "content": {
                    "anyOf": [
                      {
                        "anyOf": [
                          {
                            "type": "string"
                          },
                          {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "type": {
                                  "type": "string",
                                  "enum": ["text", "image"]
                                },
                                "mimeType": {
                                  "type": "string"
                                },
                                "text": {
                                  "type": "string"
                                },
                                "url": {
                                  "type": "string"
                                }
                              },
                              "required": ["type"],
                              "additionalProperties": false
                            }
                          }
                        ]
                      },
                      {
                        "type": "null"
                      }
                    ]
                  },
                  "role": {
                    "type": "string",
                    "const": "assistant"
                  },
                  "index": {
                    "type": "number"
                  },
                  "stopReason": {
                    "type": "string",
                    "enum": ["stop", "max_tokens", "tool_calls", "content_filter", "other"]
                  }
                },
                "required": ["content", "role", "index", "stopReason"],
                "additionalProperties": false
              }
            },
            "usage": {
              "type": "object",
              "properties": {
                "inputTokens": {
                  "type": "number"
                },
                "inputCost": {
                  "type": "number"
                },
                "outputTokens": {
                  "type": "number"
                },
                "outputCost": {
                  "type": "number"
                }
              },
              "required": ["inputTokens", "inputCost", "outputTokens", "outputCost"],
              "additionalProperties": false
            },
            "botpress": {
              "type": "object",
              "properties": {
                "cost": {
                  "type": "number"
                }
              },
              "required": ["cost"],
              "additionalProperties": false
            }
          },
          "required": ["id", "provider", "model", "choices", "usage", "botpress"],
          "additionalProperties": false
        }
      },
      "billable": true,
      "cacheable": true
    },
    "listLanguageModels": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {},
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "models": {
              "type": "array",
              "items": {
                "allOf": [
                  {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      },
                      "tags": {
                        "type": "array",
                        "items": {
                          "type": "string",
                          "enum": [
                            "recommended",
                            "deprecated",
                            "general-purpose",
                            "low-cost",
                            "vision",
                            "coding",
                            "agents",
                            "function-calling",
                            "roleplay",
                            "storytelling",
                            "reasoning",
                            "preview"
                          ]
                        }
                      },
                      "input": {
                        "type": "object",
                        "properties": {
                          "maxTokens": {
                            "type": "number"
                          },
                          "costPer1MTokens": {
                            "type": "number"
                          }
                        },
                        "required": ["maxTokens", "costPer1MTokens"],
                        "additionalProperties": false
                      },
                      "output": {
                        "type": "object",
                        "properties": {
                          "maxTokens": {
                            "type": "number"
                          },
                          "costPer1MTokens": {
                            "type": "number"
                          }
                        },
                        "required": ["maxTokens", "costPer1MTokens"],
                        "additionalProperties": false
                      }
                    },
                    "required": ["id", "name", "description", "tags", "input", "output"]
                  },
                  {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "enum": ["llama3.1-8b", "llama3.3-70b"],
                        "description": "Model to use for content generation",
                        "x-zui": {
                          "placeholder": "llama3.1-8b"
                        }
                      }
                    },
                    "required": ["id"]
                  }
                ]
              }
            }
          },
          "required": ["models"],
          "additionalProperties": false
        }
      }
    }
  },
  "channels": {},
  "entities": {
    "modelRef": {
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "enum": ["llama3.1-8b", "llama3.3-70b"],
            "description": "Model to use for content generation",
            "x-zui": {
              "placeholder": "llama3.1-8b"
            }
          }
        },
        "required": ["id"],
        "additionalProperties": false
      }
    }
  },
  "interfaces": {
    "llm<modelRef>": {
      "id": "",
      "name": "llm",
      "version": "8.0.0",
      "entities": {
        "modelRef": {
          "name": "modelRef"
        }
      },
      "actions": {
        "generateContent": {
          "name": "generateContent"
        },
        "listLanguageModels": {
          "name": "listLanguageModels"
        }
      },
      "events": {},
      "channels": {}
    }
  }
}
