    "conversation": {
      "title": "Conversation",
      "description": "A Sunshine Conversations conversation",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The Sunshine Conversations conversation ID",
            "x-zui": {
              "title": "ID"
            }
          }
        },
        "required": ["id"],
        "additionalProperties": false,
        "description": "The conversation object fields",
        "x-zui": {
          "title": "Conversation"
        }
      }
    }
  },
  "interfaces": {
    "typingIndicator": {
      "id": "",
      "name": "typingIndicator",
      "version": "0.0.1",
      "entities": {},
      "actions": {
        "startTypingIndicator": {
          "name": "startTypingIndicator"
        },
        "stopTypingIndicator": {
          "name": "stopTypingIndicator"
        }
      },
      "events": {},
      "channels": {}
    },
    "proactiveUser<user>": {
      "id": "",
      "name": "proactiveUser",
      "version": "0.0.2",
      "entities": {
        "user": {
          "name": "user"
        }
      },
      "actions": {
        "getOrCreateUser": {
          "name": "getOrCreateUser"
        }
      },
      "events": {},
      "channels": {}
    },
    "proactiveConversation<conversation>": {
      "id": "",
      "name": "proactiveConversation",
      "version": "0.0.2",
      "entities": {
        "conversation": {
          "name": "conversation"
        }
      },
      "actions": {
        "getOrCreateConversation": {
          "name": "getOrCreateConversation"
        }
      },
      "events": {},
      "channels": {}
    }
  }
}
