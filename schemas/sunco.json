{
  "name": "sunco",
  "version": "1.0.0",
  "title": "Sunshine Conversations",
  "description": "Give your bot access to a powerful omnichannel messaging platform.",
  "user": {
    "tags": {
      "id": {}
    }
  },
  "events": {},
  "actions": {
    "startTypingIndicator": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "conversationId": {
              "type": "string"
            },
            "messageId": {
              "type": "string"
            },
            "timeout": {
              "type": "number"
            }
          },
          "required": ["conversationId", "messageId"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {},
          "additionalProperties": false
        }
      }
    },
    "stopTypingIndicator": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "conversationId": {
              "type": "string"
            },
            "messageId": {
              "type": "string"
            }
          },
          "required": ["conversationId", "messageId"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {},
          "additionalProperties": false
        }
      }
    },
    "getOrCreateUser": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "pictureUrl": {
              "type": "string"
            },
            "email": {
              "type": "string"
            },
            "user": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "description": "The Sunshine Conversations user ID",
                  "x-zui": {
                    "title": "ID"
                  }
                }
              },
              "required": ["id"],
              "additionalProperties": false,
              "description": "The user object fields",
              "x-zui": {
                "title": "User"
              }
            }
          },
          "required": ["user"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "userId": {
              "type": "string"
            }
          },
          "required": ["userId"],
          "additionalProperties": false
        }
      },
      "title": "Get or Create a User",
      "description": "Proactively create a user from a bot"
    },
    "getOrCreateConversation": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "conversation": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "description": "The Sunshine Conversations conversation ID",
                  "x-zui": {
                    "title": "ID"
                  }
                }
              },
              "required": ["id"],
              "additionalProperties": false,
              "description": "The conversation object fields",
              "x-zui": {
                "title": "Conversation"
              }
            }
          },
          "required": ["conversation"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "conversationId": {
              "type": "string"
            }
          },
          "required": ["conversationId"],
          "additionalProperties": false
        }
      }
    }
  },
  "channels": {
    "channel": {
      "messages": {
        "text": {
          "schema": {
            "type": "object",
            "properties": {
              "text": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["text"],
            "additionalProperties": false
          }
        },
        "image": {
          "schema": {
            "type": "object",
            "properties": {
              "imageUrl": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["imageUrl"],
            "additionalProperties": false
          }
        },
        "audio": {
          "schema": {
            "type": "object",
            "properties": {
              "audioUrl": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["audioUrl"],
            "additionalProperties": false
          }
        },
        "video": {
          "schema": {
            "type": "object",
            "properties": {
              "videoUrl": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["videoUrl"],
            "additionalProperties": false
          }
        },
        "file": {
          "schema": {
            "type": "object",
            "properties": {
              "fileUrl": {
                "type": "string",
                "minLength": 1
              },
              "title": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["fileUrl"],
            "additionalProperties": false
          }
        },
        "location": {
          "schema": {
            "type": "object",
            "properties": {
              "latitude": {
                "type": "number"
              },
              "longitude": {
                "type": "number"
              },
              "address": {
                "type": "string"
              },
              "title": {
                "type": "string"
              }
            },
            "required": ["latitude", "longitude"],
            "additionalProperties": false
          }
        },
        "carousel": {
          "schema": {
            "type": "object",
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "title": {
                      "type": "string",
                      "minLength": 1
                    },
                    "subtitle": {
                      "type": "string",
                      "minLength": 1
                    },
                    "imageUrl": {
                      "type": "string",
                      "minLength": 1
                    },
                    "actions": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "action": {
                            "type": "string",
                            "enum": ["postback", "url", "say"]
                          },
                          "label": {
                            "type": "string",
                            "minLength": 1
                          },
                          "value": {
                            "type": "string",
                            "minLength": 1
                          }
                        },
                        "required": ["action", "label", "value"],
                        "additionalProperties": false
                      }
                    }
                  },
                  "required": ["title", "actions"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["items"],
            "additionalProperties": false
          }
        },
        "card": {
          "schema": {
            "type": "object",
            "properties": {
              "title": {
                "type": "string",
                "minLength": 1
              },
              "subtitle": {
                "type": "string",
                "minLength": 1
              },
              "imageUrl": {
                "type": "string",
                "minLength": 1
              },
              "actions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "action": {
                      "type": "string",
                      "enum": ["postback", "url", "say"]
                    },
                    "label": {
                      "type": "string",
                      "minLength": 1
                    },
                    "value": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "required": ["action", "label", "value"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["title", "actions"],
            "additionalProperties": false
          }
        },
        "dropdown": {
          "schema": {
            "type": "object",
            "properties": {
              "text": {
                "type": "string",
                "minLength": 1
              },
              "options": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string",
                      "minLength": 1
                    },
                    "value": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "required": ["label", "value"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["text", "options"],
            "additionalProperties": false
          }
        },
        "choice": {
          "schema": {
            "type": "object",
            "properties": {
              "text": {
                "type": "string",
                "minLength": 1
              },
              "options": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string",
                      "minLength": 1
                    },
                    "value": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "required": ["label", "value"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["text", "options"],
            "additionalProperties": false
          }
        },
        "bloc": {
          "schema": {
            "type": "object",
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "anyOf": [
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "text"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "text": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["text"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "markdown"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "markdown": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["markdown"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "image"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "imageUrl": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["imageUrl"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "audio"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "audioUrl": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["audioUrl"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "video"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "videoUrl": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["videoUrl"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "file"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "fileUrl": {
                              "type": "string",
                              "minLength": 1
                            },
                            "title": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["fileUrl"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "location"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "latitude": {
                              "type": "number"
                            },
                            "longitude": {
                              "type": "number"
                            },
                            "address": {
                              "type": "string"
                            },
                            "title": {
                              "type": "string"
                            }
                          },
                          "required": ["latitude", "longitude"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    }
                  ]
                }
              }
            },
            "required": ["items"],
            "additionalProperties": false
          }
        },
        "markdown": {
          "schema": {
            "type": "object",
            "properties": {
              "markdown": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["markdown"],
            "additionalProperties": false
          }
        }
      },
      "message": {
        "tags": {
          "id": {}
        }
      },
      "conversation": {
        "tags": {
          "id": {}
        }
      }
    }
  },
  "entities": {
    "user": {
      "title": "User",
      "description": "A Sunshine Conversations user",
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The Sunshine Conversations user ID",
            "x-zui": {
              "title": "ID"
            }
          }
