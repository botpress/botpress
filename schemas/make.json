{
  "name": "make",
  "version": "0.3.4",
  "title": "Make.com (Deprecated)",
  "description": "Automate tasks, sync data across multiple applications, and enhance the capabilities of your chatbot like never before.",
  "actions": {
    "sendData": {
      "input": {
        "schema": {
          "type": "object",
          "description": "Input schema for sending data",
          "properties": {
            "data": {
              "type": "string",
              "description": "JSON string of data to send",
              "x-zui": {},
              "minLength": 1
            }
          },
          "required": [
            "data"
          ],
          "additionalProperties": false,
          "x-zui": {}
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "description": "Output schema after sending data, expecting any JSON structure",
          "properties": {
            "success": {
              "type": "boolean",
              "description": "True if the data was sent successfully",
              "x-zui": {}
            },
            "response": {
              "anyOf": [
                {
                  "description": "Data received from Make.com, will be the string `Accepted` if successful and no data is returned",
                  "x-zui": {}
                },
                {
                  "type": "null",
                  "x-zui": {}
                }
              ],
              "x-zui": {
                "def": {
                  "typeName": "ZodNullable"
                }
              }
            }
          },
          "required": [
            "success"
          ],
          "additionalProperties": false,
          "x-zui": {}
        }
      }
    }
  },
  "channels": {},
  "interfaces": {}
}
