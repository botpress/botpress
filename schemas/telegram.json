      },
      "channels": {},
      "entities": {},
      "events": {},
      "id": "",
      "name": "typingIndicator",
      "version": "0.0.1"
    }
  },
  "name": "telegram",
  "title": "Telegram",
  "user": { "creation": { "enabled": true, "requiredTags": ["id"] }, "tags": { "id": {} } },
  "version": "0.7.0"
}
