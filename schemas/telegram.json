            "required": ["fileUrl"],
            "type": "object"
          }
        },
        "image": {
          "schema": {
            "additionalProperties": false,
            "properties": { "imageUrl": { "minLength": 1, "type": "string" } },
            "required": ["imageUrl"],
            "type": "object"
          }
        },
        "location": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "address": { "type": "string" },
