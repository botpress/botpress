{
  "actions": {
    "browsePages": {
      "billable": true,
      "cacheable": true,
      "description": "Extract the full content & the metadata of the specified pages as markdown.",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "urls": { "items": { "type": "string" }, "type": "array" },
            "waitFor": {
              "default": 350,
              "description": "Time to wait before extracting the content (in milliseconds). Set this value higher for dynamic pages.",
              "type": "number"
            }
          },
          "required": ["urls"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "results": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "content": { "type": "string" },
                  "description": { "type": "string" },
                  "favicon": { "type": "string" },
                  "title": { "type": "string" },
                  "url": { "type": "string" }
                },
                "required": ["url", "content"],
                "type": "object"
              },
              "type": "array"
            }
          },
          "required": ["results"],
          "type": "object"
        }
      },
      "title": "Browse Pages"
    },
    "captureScreenshot": {
      "billable": true,
      "cacheable": true,
      "description": "Capture a screenshot of the specified page.",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": { "url": { "type": "string" } },
          "required": ["url"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": { "imageUrl": { "type": "string" } },
          "required": ["imageUrl"],
          "type": "object"
        }
      },
      "title": "Capture Screenshot"
    },
    "webSearch": {
      "cacheable": true,
      "description": "Search information on the web. You need to browse to that page to get the full content of the page.",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "browsePages": {
              "default": false,
              "description": "Whether to browse to the pages to get the full content",
              "type": "boolean"
            },
            "count": {
              "default": 10,
              "description": "Number of search results to return (default: 10)",
              "maximum": 20,
              "minimum": 1,
              "type": "number"
            },
            "excludeSites": {
              "description": "Exclude these domains from the search (max 20)",
              "items": {
                "maxLength": 50,
                "minLength": 3,
                "pattern": "^(?:[a-z0-9](?:[a-z0-9-]{0,61}[a-z0-9])?\.)+[a-z]{2,}$",
                "type": "string"
              },
              "maxItems": 20,
              "type": "array"
            },
            "freshness": {
              "description": "Only consider results from the last day, week or month",
              "enum": ["Day", "Week", "Month"],
              "type": "string"
            },
            "includeSites": {
              "description": "Include only these domains in the search (max 20)",
              "items": {
                "maxLength": 50,
                "minLength": 3,
                "pattern": "^(?:[a-z0-9](?:[a-z0-9-]{0,61}[a-z0-9])?\.)+[a-z]{2,}$",
                "type": "string"
              },
              "maxItems": 20,
              "type": "array"
            },
            "query": {
              "description": "What are we searching for?",
              "maxLength": 1000,
              "minLength": 1,
              "type": "string"
            }
          },
          "required": ["query"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "results": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "links": {
                    "description": "Useful links on the page",
                    "items": {
                      "additionalProperties": false,
                      "properties": { "name": { "type": "string" }, "url": { "type": "string" } },
                      "required": ["name", "url"],
                      "type": "object"
                    },
                    "type": "array"
                  },
                  "name": { "description": "Title of the page", "type": "string" },
                  "page": {
                    "additionalProperties": false,
                    "properties": {
                      "content": { "type": "string" },
                      "description": { "type": "string" },
                      "favicon": { "type": "string" },
                      "title": { "type": "string" },
                      "url": { "type": "string" }
                    },
                    "required": ["url", "content"],
                    "type": "object"
                  },
                  "snippet": { "description": "A short summary of the page", "type": "string" },
                  "url": { "description": "URL of the page", "type": "string" }
                },
                "required": ["name", "url", "snippet"],
                "type": "object"
              },
              "type": "array"
            }
          },
          "required": ["results"],
          "type": "object"
        }
      },
      "title": "Web Search"
    }
  },
  "description": "Capture screenshots and retrieve web page content with metadata for automated browsing and data extraction.",
  "interfaces": {},
  "name": "browser",
  "title": "Browser",
  "version": "0.4.1"
}
