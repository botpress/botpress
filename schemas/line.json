{
  "actions": {
    "startTypingIndicator": {
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "conversationId": { "type": "string" },
            "messageId": { "type": "string" },
            "timeout": { "type": "number" }
          },
          "required": ["conversationId", "messageId"],
          "type": "object"
        }
      },
      "output": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } }
    },
    "stopTypingIndicator": {
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": { "conversationId": { "type": "string" }, "messageId": { "type": "string" } },
          "required": ["conversationId", "messageId"],
          "type": "object"
        }
      },
      "output": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } }
    }
  },
  "channels": {
    "channel": {
      "conversation": {
        "creation": { "enabled": true, "requiredTags": ["usrId", "destId"] },
        "tags": {
          "destId": { "description": "Line user ID of the bot", "title": "Destination ID" },
          "usrId": { "description": "Line user ID taking part in the conversation", "title": "User ID" }
        }
      },
      "description": "Channel for a Line conversation",
      "message": { "tags": { "msgId": { "description": "Line message ID", "title": "Message ID" } } },
      "messages": {
        "audio": {
          "schema": {
            "additionalProperties": false,
            "properties": { "audioUrl": { "minLength": 1, "type": "string" } },
            "required": ["audioUrl"],
            "type": "object"
          }
        },
        "bloc": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "items": {
                "items": {
                  "anyOf": [
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "text": { "minLength": 1, "type": "string" } },
                          "required": ["text"],
                          "type": "object"
                        },
                        "type": { "const": "text", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "markdown": { "minLength": 1, "type": "string" } },
                          "required": ["markdown"],
                          "type": "object"
                        },
                        "type": { "const": "markdown", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "imageUrl": { "minLength": 1, "type": "string" } },
                          "required": ["imageUrl"],
                          "type": "object"
                        },
                        "type": { "const": "image", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "audioUrl": { "minLength": 1, "type": "string" } },
                          "required": ["audioUrl"],
                          "type": "object"
                        },
                        "type": { "const": "audio", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "videoUrl": { "minLength": 1, "type": "string" } },
                          "required": ["videoUrl"],
                          "type": "object"
                        },
                        "type": { "const": "video", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": {
                            "fileUrl": { "minLength": 1, "type": "string" },
                            "title": { "minLength": 1, "type": "string" }
                          },
                          "required": ["fileUrl"],
                          "type": "object"
                        },
                        "type": { "const": "file", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": {
                            "address": { "type": "string" },
                            "latitude": { "type": "number" },
                            "longitude": { "type": "number" },
                            "title": { "type": "string" }
                          },
                          "required": ["latitude", "longitude"],
                          "type": "object"
                        },
                        "type": { "const": "location", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    }
                  ]
                },
                "type": "array"
              }
            },
            "required": ["items"],
            "type": "object"
          }
        },
        "card": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "actions": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "action": { "enum": ["postback", "url", "say"], "type": "string" },
                    "label": { "minLength": 1, "type": "string" },
                    "value": { "minLength": 1, "type": "string" }
                  },
                  "required": ["action", "label", "value"],
                  "type": "object"
                },
                "type": "array"
              },
              "imageUrl": { "minLength": 1, "type": "string" },
              "subtitle": { "minLength": 1, "type": "string" },
              "title": { "minLength": 1, "type": "string" }
            },
            "required": ["title", "actions"],
            "type": "object"
          }
        },
        "carousel": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "items": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "actions": {
                      "items": {
                        "additionalProperties": false,
                        "properties": {
                          "action": { "enum": ["postback", "url", "say"], "type": "string" },
                          "label": { "minLength": 1, "type": "string" },
                          "value": { "minLength": 1, "type": "string" }
                        },
                        "required": ["action", "label", "value"],
                        "type": "object"
                      },
                      "type": "array"
                    },
                    "imageUrl": { "minLength": 1, "type": "string" },
                    "subtitle": { "minLength": 1, "type": "string" },
                    "title": { "minLength": 1, "type": "string" }
                  },
                  "required": ["title", "actions"],
                  "type": "object"
                },
                "type": "array"
              }
            },
            "required": ["items"],
            "type": "object"
          }
        },
        "choice": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "options": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "label": { "minLength": 1, "type": "string" },
                    "value": { "minLength": 1, "type": "string" }
                  },
                  "required": ["label", "value"],
                  "type": "object"
                },
                "type": "array"
              },
              "text": { "minLength": 1, "type": "string" }
            },
            "required": ["text", "options"],
            "type": "object"
          }
        },
        "dropdown": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "options": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "label": { "minLength": 1, "type": "string" },
                    "value": { "minLength": 1, "type": "string" }
                  },
                  "required": ["label", "value"],
                  "type": "object"
                },
                "type": "array"
              },
              "text": { "minLength": 1, "type": "string" }
            },
            "required": ["text", "options"],
            "type": "object"
          }
        },
        "file": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "fileUrl": { "minLength": 1, "type": "string" },
              "title": { "minLength": 1, "type": "string" }
            },
            "required": ["fileUrl"],
            "type": "object"
          }
        },
        "image": {
          "schema": {
            "additionalProperties": false,
            "properties": { "imageUrl": { "minLength": 1, "type": "string" } },
            "required": ["imageUrl"],
            "type": "object"
          }
        },
        "location": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "address": { "type": "string" },
              "latitude": { "type": "number" },
              "longitude": { "type": "number" },
              "title": { "type": "string" }
            },
            "required": ["latitude", "longitude"],
            "type": "object"
          }
        },
        "markdown": {
          "schema": {
            "additionalProperties": false,
            "properties": { "markdown": { "minLength": 1, "type": "string" } },
            "required": ["markdown"],
            "type": "object"
          }
        },
        "text": {
          "schema": {
            "additionalProperties": false,
            "properties": { "text": { "minLength": 1, "type": "string" } },
            "required": ["text"],
            "type": "object"
          }
        },
        "video": {
          "schema": {
            "additionalProperties": false,
            "properties": { "videoUrl": { "minLength": 1, "type": "string" } },
            "required": ["videoUrl"],
            "type": "object"
          }
        }
      },
      "title": "Line conversation"
    }
  },
  "description": "Interact with customers using a rich set of features.",
  "events": {},
  "interfaces": {
    "typingIndicator": {
      "actions": {
        "startTypingIndicator": { "name": "startTypingIndicator" },
        "stopTypingIndicator": { "name": "stopTypingIndicator" }
      },
      "channels": {},
      "entities": {},
      "events": {},
      "id": "",
      "name": "typingIndicator",
      "version": "0.0.1"
    }
  },
  "name": "line",
  "states": {
    "conversation": {
      "schema": {
        "additionalProperties": false,
        "properties": {
          "replyToken": {
            "description": "Token used to reply to a message received as a request on the webhook",
            "type": "string",
            "x-zui": { "title": "Reply Token" }
          }
        },
        "type": "object"
      },
      "type": "conversation"
    }
  },
  "title": "Line",
  "user": {
    "creation": { "enabled": true, "requiredTags": ["usrId"] },
    "tags": { "usrId": { "description": "Line user ID", "title": "User ID" } }
  },
  "version": "1.0.1"
}
