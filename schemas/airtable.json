                "required": ["_rawJson", "id"],
                "additionalProperties": false
              }
            }
          },
          "required": ["records"],
          "additionalProperties": true
        }
      }
    },
    "createTable": {
      "title": "Create Table",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "Name of the Table (e.g. MyTable)"
            },
            "fields": {
              "type": "string",
              "description": "The Table's fields, separated by commas. Each field should be in the format \"type_name\" (e.g. \"phoneNumber_Customer Phone, singleLineText_Address\")."
            },
            "description": {
              "type": "string",

              "description": "Description of the Table (e.g. This is my table) (Optional)"
            }
          },
          "required": ["name", "fields"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
