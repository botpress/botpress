    "getTableRecords": {
      "title": "Get Records of the Table",
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "tableIdOrName": {
              "type": "string",
              "description": "The ID or Name of the table (e.g. tblFnqcm4zLVKn85A or articles)"
            }
          },
          "required": ["tableIdOrName"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "records": {
              "type": "array",
