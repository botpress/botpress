      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "fields": {
              "description": "The fields and their values for the new record, in a JSON format (e.g. {\"Name\":\"John Doe\",\"City\":\"In the moon\",\"Verify\":true})",
              "type": "string"
            },
            "tableIdOrName": {
              "description": "The ID or Name of the table (e.g. tblFnqcm4zLVKn85A or articles)",
              "type": "string"
            }
          },
          "required": ["tableIdOrName", "fields"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": true,
          "properties": {
            "_rawJson": { "additionalProperties": true, "properties": {}, "type": "object" },
