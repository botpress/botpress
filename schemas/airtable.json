            },
            "name": { "description": "Name of the Table (e.g. MyTable)", "type": "string" }
          },
          "required": ["name", "fields"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": true,
          "properties": {
            "description": { "type": "string" },
            "fields": {
              "items": {
                "additionalProperties": true,
