              "type": "string",
              "x-zui": {
                "title": "Name"
              }
            },
            "notes": {
              "description": "The description of the task (Optional) (e.g. \"This is my other task created using the Asana API\")",
              "type": "string",
              "x-zui": {
                "title": "Notes"
