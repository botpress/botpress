{
  "actions": {
    "generateContent": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "debug": { "type": "boolean" },
            "maxTokens": { "type": "number" },
            "messages": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "content": {
                    "anyOf": [
                      {
                        "anyOf": [
                          { "type": "string" },
                          {
                            "items": {
                              "additionalProperties": false,
                              "properties": {
                                "mimeType": { "type": "string" },
                                "text": { "type": "string" },
                                "type": { "enum": ["text", "image"], "type": "string" },
                                "url": { "type": "string" }
                              },
                              "required": ["type"],
                              "type": "object"
                            },
                            "type": "array"
                          }
                        ]
                      },
                      { "type": "null" }
                    ]
                  },
                  "role": { "enum": ["user", "assistant"], "type": "string" },
                  "toolCalls": {
                    "items": {
                      "additionalProperties": false,
                      "properties": {
                        "function": {
                          "additionalProperties": false,
                          "properties": {
                            "arguments": {
                              "anyOf": [{ "additionalProperties": {}, "type": "object" }, { "type": "null" }]
                            },
                            "name": { "type": "string" }
                          },
                          "required": ["name", "arguments"],
                          "type": "object"
                        },
                        "id": { "type": "string" },
                        "type": { "const": "function", "type": "string" }
                      },
                      "required": ["id", "type", "function"],
                      "type": "object"
                    },
                    "type": "array"
                  },
                  "toolResultCallId": { "type": "string" },
                  "type": {
                    "default": "text",
                    "enum": ["text", "tool_calls", "tool_result", "multipart"],
                    "type": "string"
                  }
                },
                "required": ["role", "content"],
                "type": "object"
              },
              "type": "array"
            },
            "meta": {
              "additionalProperties": false,
              "properties": {
                "integrationName": { "type": "string" },
                "promptCategory": { "type": "string" },
                "promptSource": { "type": "string" }
              },
              "type": "object"
            },
            "model": {
              "additionalProperties": false,
              "properties": {
                "id": {
                  "description": "Model to use for content generation",
                  "enum": [
                    "o3-mini-2025-01-31",
                    "o1-2024-12-17",
                    "o1-mini-2024-09-12",
                    "gpt-4o-mini-2024-07-18",
                    "gpt-4o-2024-11-20",
                    "gpt-4o-2024-08-06",
                    "gpt-4o-2024-05-13",
                    "gpt-4-turbo-2024-04-09",
                    "gpt-3.5-turbo-0125"
                  ],
                  "type": "string",
                  "x-zui": { "placeholder": "gpt-4o-mini-2024-07-18" }
                }
              },
              "required": ["id"],
              "type": "object"
            },
            "reasoningEffort": { "enum": ["low", "medium", "high"], "type": "string" },
            "responseFormat": { "enum": ["text", "json_object"], "type": "string" },
            "stopSequences": { "items": { "type": "string" }, "type": "array" },
            "systemPrompt": { "type": "string" },
            "temperature": { "default": 1, "type": "number" },
            "toolChoice": {
              "additionalProperties": false,
              "properties": {
                "functionName": { "type": "string" },
                "type": { "enum": ["auto", "specific", "any", "none", ""], "type": "string" }
              },
              "type": "object"
            },
            "tools": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "function": {
                    "additionalProperties": false,
                    "properties": {
                      "argumentsSchema": { "additionalProperties": false, "properties": {}, "type": "object" },
                      "description": { "type": "string" },
                      "name": { "type": "string" }
                    },
                    "required": ["name"],
                    "type": "object"
                  },
                  "type": { "const": "function", "type": "string" }
                },
                "required": ["type", "function"],
                "type": "object"
              },
              "type": "array"
            },
            "topP": { "default": 1, "type": "number" },
            "userId": { "type": "string" }
          },
          "required": ["messages"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "botpress": {
              "additionalProperties": false,
              "properties": { "cost": { "type": "number" } },
              "required": ["cost"],
              "type": "object"
            },
            "choices": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "content": {
                    "anyOf": [
                      {
                        "anyOf": [
                          { "type": "string" },
                          {
                            "items": {
                              "additionalProperties": false,
                              "properties": {
                                "mimeType": { "type": "string" },
                                "text": { "type": "string" },
                                "type": { "enum": ["text", "image"], "type": "string" },
                                "url": { "type": "string" }
                              },
                              "required": ["type"],
                              "type": "object"
                            },
                            "type": "array"
                          }
                        ]
                      },
                      { "type": "null" }
                    ]
                  },
                  "index": { "type": "number" },
                  "role": { "const": "assistant", "type": "string" },
                  "stopReason": {
                    "enum": ["stop", "max_tokens", "tool_calls", "content_filter", "other"],
                    "type": "string"
                  },
                  "toolCalls": {
                    "items": {
                      "additionalProperties": false,
                      "properties": {
                        "function": {
                          "additionalProperties": false,
                          "properties": {
                            "arguments": {
                              "anyOf": [{ "additionalProperties": {}, "type": "object" }, { "type": "null" }]
                            },
                            "name": { "type": "string" }
                          },
                          "required": ["name", "arguments"],
                          "type": "object"
                        },
                        "id": { "type": "string" },
                        "type": { "const": "function", "type": "string" }
                      },
                      "required": ["id", "type", "function"],
                      "type": "object"
                    },
                    "type": "array"
                  },
                  "toolResultCallId": { "type": "string" },
                  "type": {
                    "default": "text",
                    "enum": ["text", "tool_calls", "tool_result", "multipart"],
                    "type": "string"
                  }
                },
                "required": ["content", "role", "index", "stopReason"],
                "type": "object"
              },
              "type": "array"
            },
            "id": { "type": "string" },
            "model": { "type": "string" },
            "provider": { "type": "string" },
            "usage": {
              "additionalProperties": false,
              "properties": {
                "inputCost": { "type": "number" },
                "inputTokens": { "type": "number" },
                "outputCost": { "type": "number" },
                "outputTokens": { "type": "number" }
              },
              "required": ["inputTokens", "inputCost", "outputTokens", "outputCost"],
              "type": "object"
            }
          },
          "required": ["id", "provider", "model", "choices", "usage", "botpress"],
          "type": "object"
        }
      }
    },
    "generateImage": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "expiration": { "type": "number" },
            "model": {
              "additionalProperties": false,
              "properties": { "id": { "type": "string" } },
              "required": ["id"],
              "type": "object"
            },
            "params": {
              "additionalProperties": false,
              "properties": {
                "style": {
                  "default": "vivid",
                  "description": "Image style - Only supported by DALL-E 3 models",
                  "enum": ["natural", "vivid"],
                  "type": "string"
                },
                "user": {
                  "description": "User ID to associate with the image, for abuse detection purposes",
                  "type": "string"
                }
              },
              "type": "object"
            },
            "prompt": { "type": "string" },
            "size": { "type": "string" }
          },
          "required": ["prompt"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "botpress": {
              "additionalProperties": false,
              "properties": { "cost": { "type": "number" } },
              "required": ["cost"],
              "type": "object"
            },
            "cost": { "type": "number" },
            "imageUrl": { "type": "string" },
            "model": { "type": "string" }
          },
          "required": ["model", "imageUrl", "cost", "botpress"],
          "type": "object"
        }
      }
    },
    "generateSpeech": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "expiration": {
              "description": "Expiration of the generated audio file in days, after which the file will be automatically deleted to free up storage space in your account. The default is to keep the file indefinitely (no expiration). The minimum is 30 days and the maximum is 90 days.",
              "maximum": 90,
              "minimum": 30,
              "type": "integer"
            },
            "format": {
              "enum": ["mp3", "opus", "aac", "flac", "wav", "pcm"],
              "type": "string",
              "x-zui": { "placeholder": "mp3" }
            },
            "input": { "type": "string" },
            "model": { "enum": ["tts-1", "tts-1-hd"], "type": "string", "x-zui": { "placeholder": "tts-1" } },
            "speed": { "maximum": 4, "minimum": 0.25, "type": "number", "x-zui": { "placeholder": "1" } },
            "voice": {
              "enum": ["alloy", "echo", "fable", "onyx", "nova", "shimmer"],
              "type": "string",
              "x-zui": { "placeholder": "alloy" }
            }
          },
          "required": ["input"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "audioUrl": { "description": "URL to the audio file with the generated speech", "type": "string" },
            "botpress": {
              "additionalProperties": false,
              "properties": {
                "cost": { "description": "Cost of the speech generation, in U.S. dollars", "type": "number" }
              },
              "required": ["cost"],
              "type": "object"
            }
          },
          "required": ["audioUrl", "botpress"],
          "type": "object"
        }
      }
    },
    "listImageModels": {
      "input": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "models": {
              "items": {
                "allOf": [
                  {
                    "properties": {
                      "costPerImage": { "type": "number" },
                      "defaultSize": { "type": "string" },
                      "id": { "type": "string" },
                      "name": { "type": "string" },
                      "sizes": { "items": { "type": "string" }, "type": "array" }
                    },
                    "required": ["id", "name", "costPerImage", "sizes", "defaultSize"],
                    "type": "object"
                  },
                  { "properties": { "id": { "type": "string" } }, "required": ["id"], "type": "object" }
                ]
              },
              "type": "array"
            }
          },
          "required": ["models"],
          "type": "object"
        }
      }
    },
    "listLanguageModels": {
      "input": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "models": {
              "items": {
                "allOf": [
                  {
                    "properties": {
                      "description": { "type": "string" },
                      "id": { "type": "string" },
                      "input": {
                        "additionalProperties": false,
                        "properties": { "costPer1MTokens": { "type": "number" }, "maxTokens": { "type": "number" } },
                        "required": ["maxTokens", "costPer1MTokens"],
                        "type": "object"
                      },
                      "name": { "type": "string" },
                      "output": {
                        "additionalProperties": false,
                        "properties": { "costPer1MTokens": { "type": "number" }, "maxTokens": { "type": "number" } },
                        "required": ["maxTokens", "costPer1MTokens"],
                        "type": "object"
                      },
                      "tags": {
                        "items": {
                          "enum": [
                            "recommended",
                            "deprecated",
                            "general-purpose",
                            "low-cost",
                            "vision",
                            "coding",
                            "agents",
                            "function-calling",
                            "roleplay",
                            "storytelling",
                            "reasoning",
                            "preview"
                          ],
                          "type": "string"
                        },
                        "type": "array"
                      }
                    },
                    "required": ["id", "name", "description", "tags", "input", "output"],
                    "type": "object"
                  },
                  {
                    "properties": {
                      "id": {
                        "description": "Model to use for content generation",
                        "enum": [
                          "o3-mini-2025-01-31",
                          "o1-2024-12-17",
                          "o1-mini-2024-09-12",
                          "gpt-4o-mini-2024-07-18",
                          "gpt-4o-2024-11-20",
                          "gpt-4o-2024-08-06",
                          "gpt-4o-2024-05-13",
                          "gpt-4-turbo-2024-04-09",
                          "gpt-3.5-turbo-0125"
                        ],
                        "type": "string",
                        "x-zui": { "placeholder": "gpt-4o-mini-2024-07-18" }
                      }
                    },
                    "required": ["id"],
                    "type": "object"
                  }
                ]
              },
              "type": "array"
            }
          },
          "required": ["models"],
          "type": "object"
        }
      }
    },
    "listSpeechToTextModels": {
      "input": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "models": {
              "items": {
                "allOf": [
                  {
                    "properties": {
                      "costPerMinute": { "type": "number" },
                      "id": { "type": "string" },
                      "name": { "type": "string" }
                    },
                    "required": ["id", "name", "costPerMinute"],
                    "type": "object"
                  },
                  { "properties": { "id": { "type": "string" } }, "required": ["id"], "type": "object" }
                ]
              },
              "type": "array"
            }
          },
          "required": ["models"],
          "type": "object"
        }
      }
    },
    "transcribeAudio": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "fileUrl": { "type": "string" },
            "language": { "type": "string" },
            "model": {
              "additionalProperties": false,
              "properties": { "id": { "type": "string" } },
              "required": ["id"],
              "type": "object"
            },
            "prompt": { "type": "string" },
            "temperature": { "default": 0, "type": "number" }
          },
          "required": ["fileUrl"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "botpress": {
              "additionalProperties": false,
              "properties": { "cost": { "type": "number" } },
              "required": ["cost"],
              "type": "object"
            },
            "cost": { "type": "number" },
            "duration": { "type": "number" },
            "language": { "type": "string" },
            "model": { "type": "string" },
            "segments": {
              "items": {
                "additionalProperties": false,
                "properties": {
                  "avg_logprob": { "type": "number" },
                  "compression_ratio": { "type": "number" },
                  "end": { "type": "number" },
                  "id": { "type": "number" },
                  "no_speech_prob": { "type": "number" },
                  "seek": { "type": "number" },
                  "start": { "type": "number" },
                  "temperature": { "type": "number" },
                  "text": { "type": "string" },
                  "tokens": { "items": { "type": "number" }, "type": "array" }
                },
                "required": [
                  "text",
                  "id",
                  "seek",
                  "start",
                  "end",
                  "tokens",
                  "temperature",
                  "avg_logprob",
                  "compression_ratio",
                  "no_speech_prob"
                ],
                "type": "object"
              },
              "type": "array"
            }
          },
          "required": ["language", "duration", "segments", "model", "cost", "botpress"],
          "type": "object"
        }
      }
    }
  },
  "channels": {},
  "description": "Gain access to OpenAI models for text generation, speech synthesis, audio transcription, and image generation.",
  "entities": {
    "imageGenerationParams": {
      "schema": {
        "additionalProperties": false,
        "properties": {
          "style": {
            "default": "vivid",
            "description": "Image style - Only supported by DALL-E 3 models",
            "enum": ["natural", "vivid"],
            "type": "string"
          },
          "user": {
            "description": "User ID to associate with the image, for abuse detection purposes",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "imageModelRef": {
      "schema": {
        "additionalProperties": false,
        "properties": { "id": { "type": "string" } },
        "required": ["id"],
        "type": "object"
      }
    },
    "modelRef": {
      "schema": {
        "additionalProperties": false,
        "properties": {
          "id": {
            "description": "Model to use for content generation",
            "enum": [
              "o3-mini-2025-01-31",
              "o1-2024-12-17",
              "o1-mini-2024-09-12",
              "gpt-4o-mini-2024-07-18",
              "gpt-4o-2024-11-20",
              "gpt-4o-2024-08-06",
              "gpt-4o-2024-05-13",
              "gpt-4-turbo-2024-04-09",
              "gpt-3.5-turbo-0125"
            ],
            "type": "string",
            "x-zui": { "placeholder": "gpt-4o-mini-2024-07-18" }
          }
        },
        "required": ["id"],
        "type": "object"
      }
    },
    "speechToTextModelRef": {
      "schema": {
        "additionalProperties": false,
        "properties": { "id": { "type": "string" } },
        "required": ["id"],
        "type": "object"
      }
    }
  },
  "events": {},
  "interfaces": {
    "llm<modelRef>": {
      "actions": {
        "generateContent": { "name": "generateContent" },
        "listLanguageModels": { "name": "listLanguageModels" }
      },
      "channels": {},
      "entities": { "modelRef": { "name": "modelRef" } },
      "events": {},
      "id": "",
      "name": "llm",
      "version": "8.0.0"
    },
    "speechToText<speechToTextModelRef>": {
      "actions": {
        "listSpeechToTextModels": { "name": "listSpeechToTextModels" },
        "transcribeAudio": { "name": "transcribeAudio" }
      },
      "channels": {},
      "entities": { "speechToTextModelRef": { "name": "speechToTextModelRef" } },
      "events": {},
      "id": "",
      "name": "speechToText",
      "version": "2.0.0"
    },
    "textToImage<imageModelRef,imageGenerationParams>": {
      "actions": { "generateImage": { "name": "generateImage" }, "listImageModels": { "name": "listImageModels" } },
      "channels": {},
      "entities": {
        "imageGenerationParams": { "name": "imageGenerationParams" },
        "imageModelRef": { "name": "imageModelRef" }
      },
      "events": {},
      "id": "",
      "name": "textToImage",
      "version": "2.1.0"
    }
  },
  "name": "openai",
  "title": "OpenAI",
  "version": "13.0.0"
}
