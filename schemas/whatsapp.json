        "properties": {
          "accessToken": { "type": "string" },
          "phoneNumberId": { "type": "string" },
          "wabaId": { "type": "string" }
        },
        "type": "object"
      },
      "type": "integration"
    }
  },
  "title": "WhatsApp",
  "user": { "tags": { "name": {}, "userId": {} } },
  "version": "3.0.0"
}
