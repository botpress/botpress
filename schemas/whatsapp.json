{
  "actions": {
    "startConversation": {
      "description": "Proactively starts a conversation with a user's Whatsapp phone number by sending them a message using a Whatsapp Message Template.",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "senderPhoneNumberId": {
              "description": "Whatsapp Phone Number ID to use as sender. If not provided it defaults to the one set in the configuration.",
              "type": "string"
            },
            "templateLanguage": {
              "description": "Language of the Whatsapp Message Template to start the conversation with. Defaults to \"en_US\" (U.S. English).",
              "type": "string"
            },
            "templateName": {
              "description": "Name of the Whatsapp Message Template to start the conversation with.",
              "type": "string"
            },
            "templateVariablesJson": {
              "description": "JSON array representation of variable values to pass to the Whatsapp Message Template.",
              "type": "string"
            },
            "userPhone": {
              "description": "Phone number of the Whatsapp user to start the conversation with.",
              "type": "string"
            }
          },
          "required": ["userPhone", "templateName"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": { "conversationId": { "type": "string" } },
          "required": ["conversationId"],
          "type": "object"
        }
      },
      "title": "Start Conversation"
    },
    "startTypingIndicator": {
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "conversationId": { "type": "string" },
            "messageId": { "type": "string" },
            "timeout": { "type": "number" }
          },
          "required": ["conversationId", "messageId"],
          "type": "object"
        }
      },
      "output": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } }
    },
    "stopTypingIndicator": {
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": { "conversationId": { "type": "string" }, "messageId": { "type": "string" } },
          "required": ["conversationId", "messageId"],
          "type": "object"
        }
      },
      "output": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } }
    }
  },
  "channels": {
    "channel": {
      "conversation": {
        "tags": {
          "phoneNumberId": {
            "description": "Whatsapp Phone Number ID to use as sender. If not provided it defaults to the one set in the configuration.",
            "title": "Phone Number ID"
          },
          "templateLanguage": {
            "description": "Language of the Whatsapp Message Template to start the conversation with. Defaults to \"en_US\" (U.S. English).",
            "title": "Message Template language (optional)"
          },
          "templateName": {
            "description": "Name of the Whatsapp Message Template to start the conversation with.",
            "title": "Message Template name"
          },
          "templateVariables": {
            "description": "JSON array representation of variable values to pass to the Whatsapp Message Template.",
            "title": "Message Template variables (optional)"
          },
          "userPhone": {
            "description": "Phone number of the Whatsapp user to start the conversation with.",
            "title": "User phone number"
          }
        }
      },
      "message": { "tags": { "id": {} } },
      "messages": {
        "audio": {
          "schema": {
            "additionalProperties": false,
            "properties": { "audioUrl": { "minLength": 1, "type": "string" } },
            "required": ["audioUrl"],
            "type": "object"
          }
        },
        "bloc": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "items": {
                "items": {
                  "anyOf": [
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "text": { "minLength": 1, "type": "string" } },
                          "required": ["text"],
                          "type": "object"
                        },
                        "type": { "const": "text", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "markdown": { "minLength": 1, "type": "string" } },
                          "required": ["markdown"],
                          "type": "object"
                        },
                        "type": { "const": "markdown", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "imageUrl": { "minLength": 1, "type": "string" } },
                          "required": ["imageUrl"],
                          "type": "object"
                        },
                        "type": { "const": "image", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "audioUrl": { "minLength": 1, "type": "string" } },
                          "required": ["audioUrl"],
                          "type": "object"
                        },
                        "type": { "const": "audio", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": { "videoUrl": { "minLength": 1, "type": "string" } },
                          "required": ["videoUrl"],
                          "type": "object"
                        },
                        "type": { "const": "video", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": {
                            "fileUrl": { "minLength": 1, "type": "string" },
                            "title": { "minLength": 1, "type": "string" }
                          },
                          "required": ["fileUrl"],
                          "type": "object"
                        },
                        "type": { "const": "file", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    },
                    {
                      "additionalProperties": false,
                      "properties": {
                        "payload": {
                          "additionalProperties": false,
                          "properties": {
                            "address": { "type": "string" },
                            "latitude": { "type": "number" },
                            "longitude": { "type": "number" },
                            "title": { "type": "string" }
                          },
                          "required": ["latitude", "longitude"],
                          "type": "object"
                        },
                        "type": { "const": "location", "type": "string" }
                      },
                      "required": ["type", "payload"],
                      "type": "object"
                    }
                  ]
                },
                "type": "array"
              }
            },
            "required": ["items"],
            "type": "object"
          }
        },
        "card": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "actions": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "action": { "enum": ["postback", "url", "say"], "type": "string" },
                    "label": { "minLength": 1, "type": "string" },
                    "value": { "minLength": 1, "type": "string" }
                  },
                  "required": ["action", "label", "value"],
                  "type": "object"
                },
                "type": "array"
              },
              "imageUrl": { "minLength": 1, "type": "string" },
              "subtitle": { "minLength": 1, "type": "string" },
              "title": { "minLength": 1, "type": "string" }
            },
            "required": ["title", "actions"],
            "type": "object"
          }
        },
        "carousel": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "items": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "actions": {
                      "items": {
                        "additionalProperties": false,
                        "properties": {
                          "action": { "enum": ["postback", "url", "say"], "type": "string" },
                          "label": { "minLength": 1, "type": "string" },
                          "value": { "minLength": 1, "type": "string" }
                        },
                        "required": ["action", "label", "value"],
                        "type": "object"
                      },
                      "type": "array"
                    },
                    "imageUrl": { "minLength": 1, "type": "string" },
                    "subtitle": { "minLength": 1, "type": "string" },
                    "title": { "minLength": 1, "type": "string" }
                  },
                  "required": ["title", "actions"],
                  "type": "object"
                },
                "type": "array"
              }
            },
            "required": ["items"],
            "type": "object"
          }
        },
        "choice": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "options": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "label": { "minLength": 1, "type": "string" },
                    "value": { "minLength": 1, "type": "string" }
                  },
                  "required": ["label", "value"],
                  "type": "object"
                },
                "type": "array"
              },
              "text": { "minLength": 1, "type": "string" }
            },
            "required": ["text", "options"],
            "type": "object"
          }
        },
        "dropdown": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "options": {
                "items": {
                  "additionalProperties": false,
                  "properties": {
                    "label": { "minLength": 1, "type": "string" },
                    "value": { "minLength": 1, "type": "string" }
                  },
                  "required": ["label", "value"],
                  "type": "object"
                },
                "type": "array"
              },
              "text": { "minLength": 1, "type": "string" }
            },
            "required": ["text", "options"],
            "type": "object"
          }
        },
        "file": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "fileUrl": { "minLength": 1, "type": "string" },
              "filename": { "type": "string" },
              "title": { "minLength": 1, "type": "string" }
            },
            "required": ["fileUrl"],
            "type": "object"
          }
        },
        "image": {
          "schema": {
            "additionalProperties": false,
            "properties": { "imageUrl": { "minLength": 1, "type": "string" } },
            "required": ["imageUrl"],
            "type": "object"
          }
        },
        "location": {
          "schema": {
            "additionalProperties": false,
            "properties": {
              "address": { "type": "string" },
              "latitude": { "type": "number" },
              "longitude": { "type": "number" },
              "title": { "type": "string" }
            },
            "required": ["latitude", "longitude"],
            "type": "object"
          }
        },
        "markdown": {
          "schema": {
            "additionalProperties": false,
            "properties": { "markdown": { "minLength": 1, "type": "string" } },
            "required": ["markdown"],
            "type": "object"
          }
        },
        "text": {
          "schema": {
            "additionalProperties": false,
            "properties": { "text": { "minLength": 1, "type": "string" } },
            "required": ["text"],
            "type": "object"
          }
        },
        "video": {
          "schema": {
            "additionalProperties": false,
            "properties": { "videoUrl": { "minLength": 1, "type": "string" } },
            "required": ["videoUrl"],
            "type": "object"
          }
        }
      }
    }
  },
  "description": "Send and receive messages through WhatsApp.",
  "events": {},
  "interfaces": {
    "typingIndicator": {
      "actions": {
