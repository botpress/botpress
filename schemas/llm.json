{
  "name": "llm",
  "version": "8.0.0",
  "entities": {
    "modelRef": {
      "schema": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          }
        },
        "required": [
          "id"
        ]
      }
    }
  },
  "events": {},
  "actions": {
    "generateContent": {
      "billable": true,
      "cacheable": true,
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "model": {
              "$ref": "modelRef",
              "description": "Model to use for content generation"
            },
            "reasoningEffort": {
              "type": "string",
              "enum": [
                "low",
                "medium",
                "high"
              ]
            },
            "systemPrompt": {
              "type": "string"
            },
            "messages": {
              "type": "array",
              "description": "Array of messages for the model to process",
              "items": {
                "type": "object",
                "properties": {
                  "role": {
                    "type": "string",
                    "enum": [
                      "user",
                      "assistant"
                    ]
                  },
                  "type": {
                    "type": "string",
                    "enum": [
                      "text",
                      "tool_calls",
                      "tool_result",
                      "multipart"
                    ],
                    "default": "text"
                  },
                  "toolCalls": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string"
                        },
                        "type": {
                          "type": "string",
                          "enum": [
                            "function"
                          ]
                        },
                        "function": {
                          "type": "object",
                          "properties": {
                            "name": {
                              "type": "string"
                            },
                            "arguments": {
                              "anyOf": [
                                {
                                  "type": "object",
                                  "additionalProperties": {}
                                },
                                {
                                  "type": "null"
                                }
                              ],
                              "x-zui": {
                                "def": {
                                  "typeName": "ZodNullable"
                                }
                              }
                            }
                          },
                          "required": [
                            "name",
                            "arguments"
                          ]
                        }
                      },
                      "required": [
                        "id",
                        "type",
                        "function"
                      ]
                    }
                  },
                  "toolResultCallId": {
                    "type": "string"
                  },
                  "content": {
                    "anyOf": [
                      {
                        "anyOf": [
                          {
                            "type": "string"
                          },
                          {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "type": {
                                  "type": "string",
                                  "enum": [
                                    "text",
                                    "image"
                                  ]
                                },
                                "mimeType": {
                                  "type": "string"
                                },
                                "text": {
                                  "type": "string"
                                },
                                "url": {
                                  "type": "string"
                                }
                              },
                              "required": [
                                "type"
                              ]
                            }
                          }
                        ]
                      },
                      {
                        "type": "null"
                      }
                    ],
                    "x-zui": {
                      "def": {
                        "typeName": "ZodNullable"
                      }
                    }
                  }
                },
                "required": [
                  "role",
                  "content"
                ]
              }
            },
            "responseFormat": {
              "type": "string",
              "enum": [
                "text",
                "json_object"
              ]
            },
            "maxTokens": {
              "type": "number"
            },
            "temperature": {
              "type": "number",
              "x-zui": {
                "displayAs": [
                  "slider",
                  {
                    "stepSize": 0.01,
                    "horizontal": true
                  }
                ]
              },
              "minimum": 0,
              "maximum": 2,
              "default": 1
            },
            "topP": {
              "type": "number",
              "minimum": 0,
              "maximum": 1,
              "default": 1
            },
            "stopSequences": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "maxItems": 4
            },
            "tools": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "const": "function"
                  },
                  "function": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string",
                        "description": "Function name"
                      },
                      "description": {
                        "type": "string"
                      },
                      "argumentsSchema": {
                        "type": "object",
                        "properties": {},
                        "required": [],
                        "additionalProperties": {}
                      }
                    },
                    "required": [
                      "name"
                    ]
                  }
                },
                "required": [
                  "type",
                  "function"
                ]
              }
            },
            "toolChoice": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "enum": [
                    "auto",
                    "specific",
                    "any",
                    "none",
                    ""
                  ]
                },
                "functionName": {
                  "type": "string"
                }
              },
              "required": []
            },
            "userId": {
              "type": "string"
            },
            "debug": {
              "type": "boolean",
              "x-zui": {
                "hidden": true
              }
            },
            "meta": {
              "type": "object",
              "properties": {
                "promptSource": {
                  "type": "string"
                },
                "promptCategory": {
                  "type": "string"
                },
                "integrationName": {
                  "type": "string"
                }
              },
              "required": [],
              "x-zui": {
                "hidden": true
              }
            }
          },
          "required": [
            "messages"
          ]
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string",
              "description": "Response ID from LLM provider"
            },
            "provider": {
              "type": "string",
              "description": "LLM provider name"
            },
            "model": {
              "type": "string",
              "description": "Model name"
            },
            "choices": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": [
                      "text",
                      "tool_calls",
                      "tool_result",
                      "multipart"
                    ],
                    "default": "text"
                  },
                  "toolCalls": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string"
                        },
                        "type": {
                          "type": "string",
                          "enum": [
                            "function"
                          ]
                        },
                        "function": {
                          "type": "object",
                          "properties": {
                            "name": {
                              "type": "string"
                            },
                            "arguments": {
                              "anyOf": [
                                {
                                  "type": "object",
                                  "additionalProperties": {}
                                },
                                {
                                  "type": "null"
                                }
                              ],
                              "x-zui": {
                                "def": {
                                  "typeName": "ZodNullable"
                                }
                              }
                            }
                          },
                          "required": [
                            "name",
                            "arguments"
                          ]
                        }
                      },
                      "required": [
                        "id",
                        "type",
                        "function"
                      ]
                    }
                  },
                  "toolResultCallId": {
                    "type": "string"
                  },
                  "content": {
                    "anyOf": [
                      {
                        "anyOf": [
                          {
                            "type": "string"
                          },
                          {
                            "type": "array",
                            "items": {
                              "type": "object",
                              "properties": {
                                "type": {
                                  "type": "string",
                                  "enum": [
                                    "text",
                                    "image"
                                  ]
                                },
                                "mimeType": {
                                  "type": "string"
                                },
                                "text": {
                                  "type": "string"
                                },
                                "url": {
                                  "type": "string"
                                }
                              },
                              "required": [
                                "type"
                              ]
                            }
                          }
                        ]
                      },
                      {
                        "type": "null"
                      }
                    ],
                    "x-zui": {
                      "def": {
                        "typeName": "ZodNullable"
                      }
                    }
                  },
                  "role": {
                    "type": "string",
                    "const": "assistant"
                  },
                  "index": {
                    "type": "integer"
                  },
                  "stopReason": {
                    "type": "string",
                    "enum": [
                      "stop",
                      "max_tokens",
                      "tool_calls",
                      "content_filter",
                      "other"
                    ]
                  }
                },
                "required": [
                  "content",
                  "role",
                  "index",
                  "stopReason"
                ]
              }
            },
            "usage": {
              "type": "object",
              "properties": {
                "inputTokens": {
                  "type": "integer",
                  "description": "Number of input tokens used by the model"
                },
                "inputCost": {
                  "type": "number",
                  "description": "Cost of the input tokens received by the model, in U.S. dollars"
                },
                "outputTokens": {
                  "type": "integer",
                  "description": "Number of output tokens used by the model"
                },
                "outputCost": {
                  "type": "number",
                  "description": "Cost of the output tokens generated by the model, in U.S. dollars"
                }
              },
              "required": [
                "inputTokens",
                "inputCost",
                "outputTokens",
                "outputCost"
              ]
            },
            "botpress": {
              "type": "object",
              "properties": {
                "cost": {
                  "type": "number",
                  "description": "Total cost of the content generation, in U.S. dollars"
                }
              },
              "required": [
                "cost"
              ]
            }
          },
          "required": [
            "id",
            "provider",
            "model",
            "choices",
            "usage",
            "botpress"
          ]
        }
      }
    },
    "listLanguageModels": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {},
          "required": []
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "models": {
              "type": "array",
              "items": {
                "allOf": [
                  {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "description": {
                        "type": "string"
                      },
                      "tags": {
                        "type": "array",
                        "items": {
                          "type": "string",
                          "enum": [
                            "recommended",
                            "deprecated",
                            "general-purpose",
                            "low-cost",
                            "vision",
                            "coding",
                            "agents",
                            "function-calling",
                            "roleplay",
                            "storytelling",
                            "reasoning",
                            "preview"
                          ]
                        }
                      },
                      "input": {
                        "type": "object",
                        "properties": {
                          "maxTokens": {
                            "type": "integer"
                          },
                          "costPer1MTokens": {
                            "type": "number",
                            "description": "Cost per 1 million tokens, in U.S. dollars"
                          }
                        },
                        "required": [
                          "maxTokens",
                          "costPer1MTokens"
                        ]
                      },
                      "output": {
                        "type": "object",
                        "properties": {
                          "maxTokens": {
                            "type": "integer"
                          },
                          "costPer1MTokens": {
                            "type": "number",
                            "description": "Cost per 1 million tokens, in U.S. dollars"
                          }
                        },
                        "required": [
                          "maxTokens",
                          "costPer1MTokens"
                        ]
                      }
                    },
                    "required": [
                      "id",
                      "name",
                      "description",
                      "tags",
                      "input",
                      "output"
                    ]
                  },
                  {
                    "$ref": "modelRef"
                  }
                ]
              }
            }
          },
          "required": [
            "models"
          ]
        }
      }
    }
  },
  "channels": {}
}
