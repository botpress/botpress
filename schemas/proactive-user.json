{
  "actions": {
    "getOrCreateUser": {
      "description": "Proactively create a user from a bot",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "email": { "description": "The email of the user", "type": "string", "x-zui": { "title": "Email" } },
            "name": { "description": "The name of the user", "type": "string", "x-zui": { "title": "Name" } },
            "pictureUrl": {
              "description": "The URL of the user profile picture",
              "type": "string",
              "x-zui": { "title": "Picture URL" }
            },
            "user": { "$ref": "user", "description": "The user object fields", "x-zui": { "title": "User" } }
          },
          "required": ["user"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "userId": {
              "description": "The Botpress ID of the created user",
              "type": "string",
              "x-zui": { "title": "User ID" }
            }
          },
          "required": ["userId"],
          "type": "object"
        }
      },
      "title": "Get or Create a User"
    }
  },
  "channels": {},
  "entities": {
    "user": {
      "description": "A user of the system",
      "schema": {
        "additionalProperties": false,
        "description": "The user object fields",
        "properties": {},
        "type": "object",
        "x-zui": { "title": "User" }
      },
      "title": "User"
    }
  },
  "events": {},
  "name": "proactiveUser",
  "version": "0.0.2"
}
