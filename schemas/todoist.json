  "actions": {
    "changeTaskPriority": {
      "description": "Change the priority of a task",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "priority": {
              "description": "Task priority from 1 (urgent) to 4 (lowest, default value).",
              "maximum": 4,
              "minimum": 1,
              "type": "number",
              "x-zui": { "title": "Old Priority" }
            },
            "taskId": {
              "description": "The ID of the task",
              "type": "string",
              "x-zui": { "title": "Parent Task ID (optional)" }
            }
          },
          "required": ["taskId", "priority"],
          "type": "object"
        }
      },
      "output": { "schema": { "additionalProperties": false, "properties": {}, "type": "object" } },
      "title": "Change Task Priority"
    },
    "createNewTask": {
      "description": "Create a new task",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "content": {
              "description": "The text of the task. This value may contain markdown-formatted text and hyperlinks.",
              "type": "string",
              "x-zui": { "title": "Content" }
            },
            "description": {
              "description": "A description for the task. This value may contain markdown-formatted text and hyperlinks.",
              "type": "string",
              "x-zui": { "title": "Description" }
            },
            "dueDate": {
              "description": "The due date of the task. Must be in RFC3339 format in UTC. If omitted, the task will have no due date.",
              "type": "string",
              "x-zui": { "title": "Due Date" }
            },
            "labelNames": {
              "description": "The names of the labels to add to the task. If omitted, no label will be added.",
              "items": {
                "description": "The name of the label.",
                "type": "string",
                "x-zui": { "title": "Filter by Label Name (optional)" }
              },
              "type": "array",
              "x-zui": { "title": "Label Names (optional)" }
            },
            "parentTaskId": {
              "description": "The ID of the parent task. If omitted, the task will have no parent task.",
              "type": "string",
              "x-zui": { "title": "Parent Task ID (optional)" }
            },
            "priority": {
              "description": "The priority level of the task, from 1 (normal) to 4 (urgent).",
              "maximum": 4,
              "minimum": 1,
              "type": "number",
              "x-zui": { "title": "Old Priority" }
            },
            "projectId": {
              "description": "The ID of the project. If omitted, the task will be added to the user's Inbox.",
              "type": "string",
              "x-zui": { "title": "Project ID (optional)" }
            },
            "sectionId": {
              "description": "The ID of section to put task into.",
              "type": "string",
              "x-zui": { "title": "Section ID (optional)" }
            }
          },
          "required": ["content"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "taskId": {
              "description": "The ID of the task",
              "type": "string",
              "x-zui": { "title": "Parent Task ID (optional)" }
            }
          },
          "required": ["taskId"],
          "type": "object"
        }
      },
      "title": "Create New Task"
    },
