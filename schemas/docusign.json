  "interfaces": {},
  "name": "docusign",
  "states": {
    "account": {
      "schema": {
        "additionalProperties": false,
        "properties": {
          "account": {
            "anyOf": [
              {
                "additionalProperties": false,
                "description": "The docusign account's info",
                "properties": {
                  "baseUri": {
                    "description": "The base URI for the Docusign API",
                    "type": "string",
                    "x-zui": {
                      "title": "Base URI"
                    }
                  },
                  "id": {
                    "description": "The docusign user's api account id",
                    "type": "string",
                    "x-zui": {
                      "title": "API Account ID"
                    }
                  },
                  "refreshAt": {
                    "anyOf": [
                      {
                        "description": "The unix timestamp (milliseconds) that the selected account will be refreshed (Only when not explicitly selected in the config)",
                        "minimum": 0,
                        "type": "number",
                        "x-zui": {
                          "title": "Refresh At"
                        }
                      },
                      {
                        "type": "null"
                      }
                    ],
                    "description": "The unix timestamp (milliseconds) that the selected account will be refreshed (Only when not explicitly selected in the config)",
                    "x-zui": {
                      "title": "Refresh At"
                    }
                  }
                },
                "required": ["id", "baseUri", "refreshAt"],
                "type": "object",
                "x-zui": {
                  "title": "Account Info"
                }
              },
              {
                "type": "null"
              }
            ],
            "description": "The docusign account's info",
            "x-zui": {
              "title": "Account Info"
            }
          }
        },
        "required": ["account"],
        "type": "object"
      },
      "type": "integration"
    },
    "configuration": {
      "schema": {
        "additionalProperties": false,
        "properties": {
          "oauth": {
            "anyOf": [
              {
                "additionalProperties": false,
