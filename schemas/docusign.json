                "properties": {
                  "accessToken": {
                    "description": "The access token for the integration",
                    "type": "string",
                    "x-zui": {
                      "title": "Access Token"
                    }
                  },
                  "expiresAt": {
                    "description": "The expiry time of the access token represented as a Unix timestamp (milliseconds)",
