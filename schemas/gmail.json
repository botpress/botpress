{
  "name": "gmail",
  "version": "0.6.2",
  "title": "Gmail",
  "description": "Send, receive, and manage emails directly within your bot's workflow.",
  "user": {
    "tags": {
      "id": {
        "title": "Email address",
        "description": "The email address of the user"
      }
    }
  },
  "events": {},
  "actions": {},
  "channels": {
    "channel": {
      "title": "Email thread",
      "description": "Messages in an email thread",
      "messages": {
        "text": {
          "schema": {
            "type": "object",
            "properties": {
              "text": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["text"],
            "additionalProperties": false
          }
        },
        "image": {
          "schema": {
            "type": "object",
            "properties": {
              "imageUrl": {
                "type": "string",
                "minLength": 1
              },
              "title": {
                "type": "string",
                "x-zui": {
                  "title": "Alt text"
                },
                "description": "Alt text for the image"
              }
            },
            "required": ["imageUrl"],
            "additionalProperties": false
          }
        },
        "audio": {
          "schema": {
            "type": "object",
            "properties": {
              "audioUrl": {
                "type": "string",
                "minLength": 1
              },
              "title": {
                "type": "string",
                "x-zui": {
                  "title": "Title"
                },
                "description": "Title for the audio file"
              }
            },
            "required": ["audioUrl"],
            "additionalProperties": false
          }
        },
        "video": {
          "schema": {
            "type": "object",
            "properties": {
              "videoUrl": {
                "type": "string",
                "minLength": 1
              },
              "title": {
                "type": "string",
                "x-zui": {
                  "title": "Title"
                },
                "description": "Title for the video file"
              }
            },
            "required": ["videoUrl"],
            "additionalProperties": false
          }
        },
        "file": {
          "schema": {
            "type": "object",
            "properties": {
              "fileUrl": {
                "type": "string",
                "minLength": 1
              },
              "title": {
                "type": "string",
                "x-zui": {
                  "title": "Title"
                },
                "description": "Title for the file"
              }
            },
            "required": ["fileUrl"],
            "additionalProperties": false
          }
        },
        "location": {
          "schema": {
            "type": "object",
            "properties": {
              "latitude": {
                "type": "number"
              },
              "longitude": {
                "type": "number"
              },
              "address": {
                "type": "string"
              },
              "title": {
                "type": "string"
              }
            },
            "required": ["latitude", "longitude"],
            "additionalProperties": false
          }
        },
        "carousel": {
          "schema": {
            "type": "object",
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "title": {
                      "type": "string",
                      "minLength": 1
                    },
                    "subtitle": {
                      "type": "string",
                      "minLength": 1
                    },
                    "imageUrl": {
                      "type": "string",
                      "minLength": 1
                    },
                    "actions": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "action": {
                            "type": "string",
                            "enum": ["postback", "url", "say"]
                          },
                          "label": {
                            "type": "string",
                            "minLength": 1
                          },
                          "value": {
                            "type": "string",
                            "minLength": 1
                          }
                        },
                        "required": ["action", "label", "value"],
                        "additionalProperties": false
                      }
                    }
                  },
                  "required": ["title", "actions"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["items"],
            "additionalProperties": false
          }
        },
        "card": {
          "schema": {
            "type": "object",
            "properties": {
              "title": {
                "type": "string",
                "minLength": 1
              },
              "subtitle": {
                "type": "string",
                "minLength": 1
              },
              "imageUrl": {
                "type": "string",
                "minLength": 1
              },
              "actions": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "action": {
                      "type": "string",
                      "enum": ["postback", "url", "say"]
                    },
                    "label": {
                      "type": "string",
                      "minLength": 1
                    },
                    "value": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "required": ["action", "label", "value"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["title", "actions"],
            "additionalProperties": false
          }
        },
        "dropdown": {
          "schema": {
            "type": "object",
            "properties": {
              "text": {
                "type": "string",
                "minLength": 1
              },
              "options": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string",
                      "minLength": 1
                    },
                    "value": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "required": ["label", "value"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["text", "options"],
            "additionalProperties": false
          }
        },
        "choice": {
          "schema": {
            "type": "object",
            "properties": {
              "text": {
                "type": "string",
                "minLength": 1
              },
              "options": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "label": {
                      "type": "string",
                      "minLength": 1
                    },
                    "value": {
                      "type": "string",
                      "minLength": 1
                    }
                  },
                  "required": ["label", "value"],
                  "additionalProperties": false
                }
              }
            },
            "required": ["text", "options"],
            "additionalProperties": false
          }
        },
        "bloc": {
          "schema": {
            "type": "object",
            "properties": {
              "items": {
                "type": "array",
                "items": {
                  "anyOf": [
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "text"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "text": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["text"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "markdown"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "markdown": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["markdown"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "image"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "imageUrl": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["imageUrl"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "audio"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "audioUrl": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["audioUrl"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "video"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "videoUrl": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["videoUrl"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "file"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "fileUrl": {
                              "type": "string",
                              "minLength": 1
                            },
                            "title": {
                              "type": "string",
                              "minLength": 1
                            }
                          },
                          "required": ["fileUrl"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    },
                    {
                      "type": "object",
                      "properties": {
                        "type": {
                          "type": "string",
                          "const": "location"
                        },
                        "payload": {
                          "type": "object",
                          "properties": {
                            "latitude": {
                              "type": "number"
                            },
                            "longitude": {
                              "type": "number"
                            },
                            "address": {
                              "type": "string"
                            },
                            "title": {
                              "type": "string"
                            }
                          },
                          "required": ["latitude", "longitude"],
                          "additionalProperties": false
                        }
                      },
                      "required": ["type", "payload"],
                      "additionalProperties": false
                    }
                  ]
                }
              }
            },
            "required": ["items"],
            "additionalProperties": false
          }
        },
        "markdown": {
          "schema": {
            "type": "object",
            "properties": {
              "markdown": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["markdown"],
            "additionalProperties": false
          }
        }
      },
      "message": {
        "tags": {
          "id": {
            "title": "Gmail ID",
            "description": "The unique identifier of the message on Gmail"
          }
        }
      },
      "conversation": {
        "tags": {
          "id": {
            "title": "Gmail ID",
            "description": "The unique identifier of the email thread on Gmail"
          },
          "email": {
            "title": "Sender email",
            "description": "The email address of the sender"
          },
          "subject": {
            "title": "Subject",
            "description": "The subject of the email"
          },
          "references": {
            "title": "References",
            "description": "The contents of the References header field"
          },
          "cc": {
            "title": "CC",
            "description": "Co-recipients"
          }
        }
      }
    }
  },
  "states": {
    "thread": {
      "type": "conversation",
      "schema": {
        "type": "object",
        "properties": {
          "inReplyTo": {
            "type": "string",
            "x-zui": {
              "title": "In reply to"
            },
            "description": "The ID of the message this message is a reply to"
          }
        },
        "additionalProperties": false
      }
    },
    "configuration": {
      "type": "integration",
      "schema": {
        "type": "object",
        "properties": {
          "refreshToken": {
            "type": "string",
            "description": "The refresh token to use to authenticate with Gmail. It gets exchanged for a bearer token",
            "x-zui": {
              "title": "Refresh token"
            }
          },
          "lastHistoryId": {
            "type": "string",
            "x-zui": {
              "title": "History cursor"
            },
            "description": "The last history ID processed by the integration"
          }
        },
        "required": ["refreshToken"],
        "additionalProperties": false
      }
    },
    "googlePublicCertCache": {
      "type": "integration",
      "schema": {
        "type": "object",
        "properties": {
          "certificates": {
            "type": "string",
            "description": "The certs used by Google for federated sign-on, stringified as JSON",
            "x-zui": {
              "title": "Certificates JSON"
            }
          }
        },
        "required": ["certificates"],
        "additionalProperties": false
      }
    }
  },
  "interfaces": {}
}
