{
  "name": "dalle",
  "version": "0.3.4",
  "title": "DALL-E (Deprecated)",
  "description": "Integrate DALL-E to generate images directly within your chatbot conversations.",
  "actions": {
    "generateImage": {
      "input": {
        "schema": {
          "type": "object",
          "properties": {
            "prompt": {
              "type": "string",
              "description": "Prompt for image generation."
            },
            "size": {
              "type": "string",

              "description": "Size of the image to generate. Can be \"1024x1024\", \"1792x1024\", or \"1024x1792\". Defaults to 1024x1024."
            },
            "quality": {
              "type": "string",

              "description": "Quality of the image to generate. Can be \"standard\" or \"hd\". Defaults to \"standard\"."
            },
            "model": {
              "type": "string",

              "description": "Model to use for image generation. Defaults to \"dall-e-3\"."
            },
            "user": {
              "type": "string",

              "description": "User ID"
            }
          },
          "required": ["prompt"],
          "additionalProperties": false
        }
      },
      "output": {
        "schema": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string",
              "format": "uri"
            },
            "createdDate": {
              "type": "string"
            }
          },
          "required": ["url", "createdDate"],
          "additionalProperties": false
        }
      }
    }
  },
  "channels": {},
  "interfaces": {}
}
