{
  "actions": {
    "generateImage": {
      "description": "Generate an image",
      "input": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "model": {
              "description": "Model to use for image generation. Defaults to \"dall-e-3\".",
              "type": "string",
              "x-zui": {
                "title": "Model"
              }
            },
            "prompt": {
              "description": "Prompt for image generation.",
              "type": "string",
              "x-zui": {
                "title": "Prompt"
              }
            },
            "quality": {
              "description": "Quality of the image to generate. Can be \"standard\" or \"hd\". Defaults to \"standard\".",
              "type": "string",
              "x-zui": {
                "title": "Quality"
              }
            },
            "size": {
              "description": "Size of the image to generate. Can be \"1024x1024\", \"1792x1024\", or \"1024x1792\". Defaults to 1024x1024.",
              "type": "string",
              "x-zui": {
                "title": "Size"
              }
            },
            "user": {
              "description": "User ID",
              "type": "string",
              "x-zui": {
                "title": "User"
              }
            }
          },
          "required": ["prompt"],
          "type": "object"
        }
      },
      "output": {
        "schema": {
          "additionalProperties": false,
          "properties": {
            "createdDate": {
              "description": "The date the image was created",
              "type": "string",
              "x-zui": {
                "title": "Created Date"
              }
            },
            "url": {
              "description": "The url of the generated image",
              "format": "uri",
              "type": "string",
              "x-zui": {
                "title": "Url"
              }
            }
          },
          "required": ["url", "createdDate"],
          "type": "object"
        }
      },
      "title": "generate Image"
    }
  },
  "channels": {},
  "description": "Integrate DALL-E to generate images directly within your chatbot conversations.",
  "interfaces": {},
  "name": "dalle",
  "title": "DALL-E (Deprecated)",
  "version": "0.3.5"
}
